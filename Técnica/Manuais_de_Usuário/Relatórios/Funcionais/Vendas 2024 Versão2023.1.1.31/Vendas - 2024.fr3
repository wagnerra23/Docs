<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Modal="False" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42145,6095804398" ReportOptions.Description.Text="[Dados]&#13;&#10;Versao=2023.0.1.19&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.18&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.17&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.16&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.15&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.14&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.13&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.11&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.10&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.9&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.8&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.7&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.6&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.5&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2023.0.1.1&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2022.0.3.14&#13;&#10;Link=&#13;&#10;Observacao=RELATORIO DE VENDAS&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo&#13;&#10;&#13;&#10;&#13;&#10;Form=TImpriVendas_Periodo" ReportOptions.LastChange="42145,6095804398" ReportOptions.VersionBuild="19" ReportOptions.VersionMajor="2023" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="1" ScriptLanguage="PascalScript" ScriptText.Text="/////////APLICA O SQL INICIAL E NECESSARIO DA VENDA\\\\\\\\\\\\\\\\\\\\\\&#13;&#10;const&#13;&#10;  ASQLSelVenda = 'select v.codigo, v.razaosocial, v.dt_emissao, v.total, v.sub_total, v.nf_dt_emissao, v.notafiscal,' +     &#13;&#10;  ' v.venda_tipo, v.dt_faturamento, v.situacaofinanceira, v.voutro, v.vdesc, v.telefone, v.pessoa_responsavel_codigo, v.quantidade,' +       &#13;&#10;  ' (''a ''||V.CODIGO||''  ''||V.RAZAOSOCIAL) AS LEGENDA,  (V.NF_VICMS+V.NF_II_VII+V.NF_IPI_VIPI+V.NF_VSEG) AS TOTAL_IMPOSTOS,' +       &#13;&#10;  ' v.PROJETO_DT_FIM, v.pedido_compra, v.dt_competencia, v.contato, V.CODUSUARIO ';&#13;&#10;Var&#13;&#10;  ACampoAgrupadorGrafico:String;                    &#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AArqINI : TIniFile;                                                            &#13;&#10;begin&#13;&#10;  AArqINI := TIniFile.create(ExtractFilePath(Application.ExeName) + '\Relatorio.ini');&#13;&#10;  try&#13;&#10;    rbCliente.Checked            := AArqINI.ReadBool('VENDAS', 'rbCliente',True);                 &#13;&#10;    RbFuncionario.Checked        := AArqINI.ReadBool('VENDAS', 'RbFuncionario',False);&#13;&#10;    RbRepresentante.Checked      := AArqINI.ReadBool('VENDAS', 'RbRepresentante',False);  &#13;&#10;    RbAgencia.Checked            := AArqINI.ReadBool('VENDAS', 'RbAgencia',False);  &#13;&#10;    RbUsuario.Checked            := AArqINI.ReadBool('VENDAS', 'RbUsuario',False);  &#13;&#10;    RbTipovenda.Checked          := AArqINI.ReadBool('VENDAS', 'RbTipovenda',False);                                             &#13;&#10;    RbGrupoCliente.Checked       := AArqINI.ReadBool('VENDAS', 'RbGrupoCliente',True);                 &#13;&#10;//    RbProduto.Checked            := AArqINI.ReadBool('VENDAS', 'RbProduto',False);&#13;&#10;//    RbGrupoProduto.Checked       := AArqINI.ReadBool('VENDAS', 'RbGrupoProduto',False);  &#13;&#10;    RbCidade.Checked             := AArqINI.ReadBool('VENDAS', 'RbCidade',False);  &#13;&#10;    RbEstado.Checked             := AArqINI.ReadBool('VENDAS', 'RbEstado',False);&#13;&#10;    RbSituacaoFinanceira.Checked := AArqINI.ReadBool('VENDAS', 'RbSituacaoFinanceira',True);                 &#13;&#10;//    Rbnaoagrupar.Checked         := AArqINI.ReadBool('VENDAS', 'Rbnaoagrupar',False);&#13;&#10;    RbCondicaoPG.Checked         := AArqINI.ReadBool('VENDAS', 'RbCondicaoPG',False);        &#13;&#10;  finally&#13;&#10;    AArqINI.Free;                    &#13;&#10;  end;                                                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnConfirmarOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AArqINI : TIniFile;        &#13;&#10;  ASQLSelVenda :String;&#13;&#10;begin&#13;&#10;  btnConfirmar.Enabled:= False;                                                &#13;&#10;  btnConfirmar.Caption:= 'Gerando...';                                                &#13;&#10;           &#13;&#10;  CarregaLogoEmpresa(Logo);                                                &#13;&#10;  AArqINI := TIniFile.create(ExtractFilePath(Application.ExeName) + '\Relatorio.ini');   // Salva última configuração, isso é muito bom                                                                                                              &#13;&#10;  try&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCliente',           RbCliente.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbFuncionario',       RbFuncionario.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbAgencia',           RbAgencia.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbRepresentante',     RbRepresentante.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbUsuario',           RbUsuario.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbTipovenda',         RbTipovenda.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbGrupoCliente',      RbGrupoCliente.Checked);&#13;&#10;//    AArqINI.WriteBool('VENDAS', 'RbProduto',           RbProduto.Checked);&#13;&#10;//    AArqINI.WriteBool('VENDAS', 'RbGrupoProduto',      RbGrupoProduto.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCidade',            RbCidade.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbEstado',            RbEstado.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbSituacaoFinanceira',RbSituacaoFinanceira.Checked);&#13;&#10;//    AArqINI.WriteBool('VENDAS', 'Rbnaoagrupar',        Rbnaoagrupar.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCondicaoPG',        RbCondicaoPG.Checked);              &#13;&#10;  finally&#13;&#10;    AArqINI.Free;                    &#13;&#10;  end;&#13;&#10;&#13;&#10; //////////////APLICA OS LEFT JOINS NECESSARIOS\\\\\\\\\\\\\\\\\\\\\\\\\                                                                                                                                                       &#13;&#10;&#13;&#10;  if (VarToStr(&#60;TipoProduto&#62;) &#60;&#62; 'TODOS') then             &#13;&#10;    Venda.SQL.Text:= SQL_Todos;            &#13;&#10;&#13;&#10;  if (RbCliente.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Por_Cliente;       &#13;&#10;&#13;&#10;  if (VarToStr(&#60;CodClienteGrupo&#62;) &#60;&#62; '') or (RbGrupoCliente.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Por_Cliente_Grupo;&#13;&#10;&#13;&#10;//  if (VarToStr(&#60;CodMarca&#62;) &#60;&#62; '') then      &#13;&#10;//    Venda.SQL.Text:= SQL_Por_Produto_Marca;&#13;&#10;            &#13;&#10;//  if (VarToStr(&#60;CodProdutoGrupo&#62;) &#60;&#62; '') or (RbGrupoProduto.Checked) then&#13;&#10;//    Venda.SQL.Text:= SQL_Por_Produto_OU_ProdutoGrupo;&#13;&#10;&#13;&#10;  if (VarToStr(&#60;CodCidade&#62;) &#60;&#62; '') or (RbCidade.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Por_Cidade_OU_CodCidade;&#13;&#10;            &#13;&#10;//  if (VarToStr(&#60;CodProduto&#62;) &#60;&#62; '') or (RbProduto.Checked) then&#13;&#10;//    Venda.SQL.Text:= SQL_Produto;      &#13;&#10;  &#13;&#10;  if (rbcondicaopg.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Condicao_Pagamento;&#13;&#10;      &#13;&#10;  if (RbTipovenda.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Venda_Tipo;&#13;&#10;  &#13;&#10;  if RbSituacaoFinanceira.checked = true then&#13;&#10;    Venda.SQL.Text:= SQL_Situacao_Financeira;        &#13;&#10;  &#13;&#10;  if (RbEstado.Checked = true) then&#13;&#10;    Venda.SQL.Text:= SQL_Estado;&#13;&#10;      &#13;&#10;  if (VarToStr(&#60;CodFornecedor&#62;) &#60;&#62; '') then&#13;&#10;    Venda.SQL.Text:= SQL_Por_Fornecedor;        &#13;&#10;  &#13;&#10;  if (RbRepresentante.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Representante;      &#13;&#10;  &#13;&#10;  if (rbfuncionario.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Funcionario;&#13;&#10;      &#13;&#10;  if (rbAgencia.Checked) then&#13;&#10;    Venda.SQL.Text:= SQL_Agencia;        &#13;&#10;                      &#13;&#10;  if (RbUsuario.checked) or (VarToStr(&#60;CodUsuario&#62;) &#60;&#62; '') then&#13;&#10;    Venda.SQL.Text:= SQL_Usuario;      &#13;&#10;    &#13;&#10;  /////////////////// FIM DOS SQLs NECESSARIOS \\\\\\\\\\\\\\\\\\\\\\\&#13;&#10;&#13;&#10;&#13;&#10;  MasterData_Grafico.Visible   := cktop10.Checked;&#13;&#10;    &#13;&#10;  DetailData_Financiero.Visible:= ckDetalhaFinanceiro.Checked;&#13;&#10;  Header_Financeiro.Visible    := ckDetalhaFinanceiro.Checked;&#13;&#10;  Footer_Financeiro.Visible    := ckDetalhaFinanceiro.Checked;        &#13;&#10;    &#13;&#10;  DetailData_Produto.Visible   := ckDetalhaProduto.Checked;&#13;&#10;  Header_DetalheProduto.Visible:= ckDetalhaProduto.Checked;&#13;&#10;  Footer_DetalheProduto.Visible:= ckDetalhaProduto.Checked;        &#13;&#10;    &#13;&#10;  Header_DetalheVenda.Visible  := ckDetalhaVenda.Checked;        &#13;&#10;  Child_DetalheDaVenda.Visible := ckDetalhaVenda.Checked;&#13;&#10;  Footer_DetalheVenda.Visible  := ckDetalhaVenda.Checked;&#13;&#10;    &#13;&#10;  Child_Cliente.Visible        := ckDetalhaCliente.Checked;&#13;&#10;  if Child_Cliente.Visible and not Child_DetalheDaVenda.Visible then&#13;&#10;    MasterData_Pedido.Child    := Child_Cliente;                              &#13;&#10;    &#13;&#10;  SumarioRelatorio.SQL.text := ' Select Sum(S.quantidade)as quantidade, SUM(S.TOTAL_IMPOSTOS)as TOTAL_IMPOSTOS, count(1), '+&#13;&#10;                               ' SUM(S.VDESC)AS VDESC, SUM(S.VOUTRO)AS VOUTRO, SUM(S.TOTAL)AS TOTAL '+       &#13;&#10;                               ' from ( '+                          &#13;&#10;                                Venda.SQL.text +&#13;&#10;                               ') S';&#13;&#10;&#13;&#10;  SumarioRelatorio.Close;&#13;&#10;  if ckDetalhaProduto.Checked then&#13;&#10;    DetailData_Produto.DataSet:= Venda_Produto&#13;&#10;  else&#13;&#10;    DetailData_Produto.DataSet:= nil;    &#13;&#10;  if ckDetalhaFinanceiro.Checked then&#13;&#10;    DetailData_Financiero.DataSet:= Venda_Financeiro&#13;&#10;  else&#13;&#10;    DetailData_Financiero.DataSet:= nil;      &#13;&#10;  if cktop10.checked then&#13;&#10;  begin              &#13;&#10;    QueryGrafico.SQL.text := ' Select (S.'+ACampoAgrupadorGrafico+')as LEGENDA, Sum(S.TOTAL)as Total '+&#13;&#10;                             ' from ( '+                          &#13;&#10;                             Venda.SQL.text +&#13;&#10;                             ') S group BY 1';&#13;&#10;                                                                                        &#13;&#10;//    ShowMessage(QueryGrafico.SQL.text);&#13;&#10;    QueryGrafico.Open;  &#13;&#10;  end;                                   &#13;&#10; //ShowMessage(Venda.SQL.text);&#13;&#10;  Venda.Open;&#13;&#10;                      &#13;&#10;  if Venda.RecordCount = 0 then&#13;&#10;    Showmessage('Não há dados para serem exibidos!');&#13;&#10;  DialogPage1.ModalResult := mrOk;    &#13;&#10;&#13;&#10;end;  &#13;&#10;    &#13;&#10;procedure btncancelarOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  DialogPage1.ModalResult     := mrCancel;&#13;&#10;  Report.PreviewOptions.Modal := False;  &#13;&#10;end;&#13;&#10;&#13;&#10; /////////////SEM SQL A FRENTE///////////////////////////&#13;&#10;&#13;&#10;procedure Prepara_Grupo(ACampoAgrupador, ACaption :String);             &#13;&#10;begin     &#13;&#10;  ACampoAgrupadorGrafico                          := ACampoAgrupador;                        &#13;&#10;  GroupHeader_Pedido.Condition                    := 'Pedidos.&#34;'+ACampoAgrupador+'&#34;';&#13;&#10;  GrupoHeader_Pedido_OrderBy_Agrupador.Lines.Text := ACaption+': [IIf( &#60;Pedidos.&#34;'+ACampoAgrupador+'&#34;&#62; = '''' , '' SEM '+ACaption+''' , &#60;Pedidos.&#34;'+ACampoAgrupador+'&#34;&#62; )]';&#13;&#10;  GroupFooter_Pedido_OrderBy_Agrupador.Lines.Text := 'TOTAL DE VENDAS - '+ACaption+': [SUM(&#60;Pedidos.&#34;TOTAL&#34;&#62;,MasterData_Pedido,1)]';&#13;&#10;end;&#13;&#10;                &#13;&#10;procedure MasterData_PedidoOnMasterDetail(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if ckDetalhaFinanceiro.checked then&#13;&#10;  begin      &#13;&#10;    DetailData_Produto.Visible := NOt(Venda.FieldByName('DT_FATURAMENTO').AsString = '');&#13;&#10;    Header_Financeiro.Visible  := NOt(Venda.FieldByName('DT_FATURAMENTO').AsString = '');                                                &#13;&#10;  end;     &#13;&#10;end;    &#13;&#10; &#13;&#10;////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;  &#13;&#10;Function SQLWhere_Por_Empresa_E_Data:String;&#13;&#10;Var&#13;&#10;  Date1 : String;&#13;&#10;  Date2 : String;&#13;&#10;  ResultFiltroDT : String;&#13;&#10;  ResultFiltros : String;        &#13;&#10;begin                   &#13;&#10;  ///////////////////////////APLICA OS SQLs WHERE DE FILTROS\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\                                                                                                                                &#13;&#10;  // FILTRO EMPRESA&#13;&#10;  Result:= '';&#13;&#10;              &#13;&#10;  if (VarToStr(&#60;CodEmpresa&#62;) &#60;&#62; '0')and(VarToStr(&#60;CodEmpresa&#62;) &#60;&#62; '') then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ '    (V.CODIGO like ''%-' + &#60;CodEmpresa&#62; + ''') ';  &#13;&#10;    EmpresaRel.SQL[3] := 'WHERE E.CODIGO = ''' + &#60;CodEmpresa&#62; + ''' ';&#13;&#10;    MemoFantasia.Lines.Text := &#60;EmpresaRel.&#34;FANTASIA&#34;&#62;;&#13;&#10;    MemoEndereco.Lines.Text := &#60;EmpresaRel.&#34;ENDERECO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;BAIRRO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;CIDADE&#34;&#62; + '/' + &#60;EmpresaRel.&#34;UF&#34;&#62; + #13#10 +      &#13;&#10;                             'CEP: ' + &#60;EmpresaRel.&#34;CEP&#34;&#62; +  ' CNPJ: ' + &#60;EmpresaRel.&#34;CNPJCPF&#34;&#62; + ' INSC: ' + &#60;EmpresaRel.&#34;IEST&#34;&#62; + #13#10 +&#13;&#10;                             'Fone: ' + &#60;EmpresaRel.&#34;FONE1&#34;&#62; + ' / Fax: ' + &#60;EmpresaRel.&#34;FAX&#34;&#62;;              &#13;&#10;  end else&#13;&#10;    ResultFiltros := ResultFiltros + ' Todas as Empresas';&#13;&#10;//  ShowMessage(VarToStr(&#60;TipoFiltroData&#62;));                        &#13;&#10;  try            &#13;&#10;    // FILTRO DATAS                                                                                                                                &#13;&#10;    Date1 := FormatDateTime('DD.MM.YYYY', &#60;DtInicio&#62;);&#13;&#10;    Date2 := FormatDateTime('DD.MM.YYYY 23:59:59', &#60;DtFim&#62;);           &#13;&#10;    case &#60;TipoFiltroData&#62; of&#13;&#10;      0 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_EMISSAO between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) ';                                            &#13;&#10;      end;            &#13;&#10;      1 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_FATURAMENTO between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) ';  &#13;&#10;      end;            &#13;&#10;      2 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_COMPETENCIA between ''' + FormatDateTime('DD.MM.YYYY', &#60;DtInicio&#62;) + ''' and coalesce(''' + FormatDateTime('DD.MM.YYYY', &#60;DtFim&#62;) + ''', current_timestamp)) ';  &#13;&#10;      end;                      &#13;&#10;    end;&#13;&#10;  except end;&#13;&#10;//  ShowMessage(Result);        &#13;&#10;  try      &#13;&#10;    //  FILTRO SELECAO (FATURADOS/NÃO FATURADOS/EXCLUIDOS)                                                                                                                                   &#13;&#10;    case &#60;TipoSelecao&#62; of              &#13;&#10;      0 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.dt_faturamento is not null) and (v.status like ''ATIVO%'') ';&#13;&#10;      end;&#13;&#10;      1 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.dt_faturamento is null) and (v.status like ''ATIVO%'') ';&#13;&#10;//      Venda_financeiro.SQL[4] := ' and (:dt_faturamento is not null)';   &#13;&#10;      end;          &#13;&#10;      2 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ 'and(v.status like ''INATIVO%'') ';&#13;&#10; //     Venda_financeiro.SQL[4] := 'and(:status like ''INATIVO%'') ';                      &#13;&#10;      end;&#13;&#10;      3 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.status like ''ATIVO%'') ';&#13;&#10;//      Venda_financeiro.SQL[4] := 'and(:status like ''ATIVO%'') ';                      &#13;&#10;      end;                      &#13;&#10;    end;&#13;&#10;  except end;&#13;&#10;&#13;&#10;// FILTROS ATIVOS /////////////////////////////////////////////////////////////////////                                                                                                                                          &#13;&#10;// FILTRO SELECAO                                           &#13;&#10;  case &#60;TipoSelecao&#62; of    &#13;&#10;    0:&#13;&#10;    begin&#13;&#10;      ResultFiltros := ResultFiltros + ' | Selecionado: Vendas Faturadas';                                                                  &#13;&#10;    end;&#13;&#10;    1:&#13;&#10;    begin&#13;&#10;      ResultFiltros := ResultFiltros + ' | Selecionado: Vendas não Faturadas';                                             &#13;&#10;    end;&#13;&#10;    2:&#13;&#10;    begin&#13;&#10;      ResultFiltros := ResultFiltros + ' | Selecionado: Vendas Canceladas/Excluídas';                                             &#13;&#10;    end;&#13;&#10;    3:&#13;&#10;    begin&#13;&#10;      ResultFiltros := ResultFiltros + ' | Selecionado: Todas as Vendas Ativas';                                             &#13;&#10;    end;&#13;&#10;  end;&#13;&#10;&#13;&#10;// FILTRO PELO MODELO DE VENDA                                                                                     &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'VENDA' then ResultFiltros := ResultFiltros + ' | Modelo: Venda';                                                                 &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'ORCAMENTO' then ResultFiltros := ResultFiltros + ' | Modelo: Orçamento';                                                                  &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'NOTA FISCAL' then ResultFiltros := ResultFiltros + ' | Modelo: Nota Fiscal';                                                               &#13;&#10;&#13;&#10;// FILTRO TIPO DE VENDA&#13;&#10;  if &#60;TipoVenda&#62; &#60;&#62; 'TODOS' then ResultFiltros := ResultFiltros + ' | Tipo de Venda: '+ &#60;TipoVenda&#62;;                                                             &#13;&#10;&#13;&#10;// FILTRO POR PRODUTO&#13;&#10;  if &#60;CodProduto&#62; &#60;&#62; '' then ShowMessage('Filtro por produto não pode ser aplicado nesse relatório. Será exibido o relatório sem este filtro.');                                                           &#13;&#10;&#13;&#10;// FILTRO POR GRUPO DE PRODUTO&#13;&#10;  if &#60;CodProdutoGrupo&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Grupo de Produto: '+ &#60;ProdutoGrupo&#62;; &#13;&#10;&#13;&#10;// FILTRO POR FUNCIONARIO                                                                                &#13;&#10;  if &#60;CodFuncionario&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Funcionário: '+ &#60;Funcionario&#62;;         &#13;&#10;&#13;&#10;// FILTRO POR REPRESENTANTE&#13;&#10;  if &#60;CodRepresentante&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Representante: '+ &#60;Representante&#62;;                                                                  &#13;&#10;&#13;&#10;// FILTRO POR MARCA&#13;&#10;  if &#60;CodMarca&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Funcionário: '+ &#60;Marca&#62;;                                                                    &#13;&#10;&#13;&#10;// FILTRO POR FORNECEDOR&#13;&#10;  if &#60;CodFornecedor&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Fornecedor: '+ &#60;Fornecedor&#62;;                                                                   &#13;&#10;&#13;&#10;// FILTRO POR CLIENTE&#13;&#10;  if &#60;CodCliente&#62; &#60;&#62; '' then ResultFiltros := ResultFiltros + ' | Cliente: '+ &#60;Cliente&#62;;                                                       &#13;&#10;&#13;&#10;// FILTRO POR GRUPO DE CLIENTE&#13;&#10;//  if &