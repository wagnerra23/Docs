<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Modal="False" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41575,6001475116" ReportOptions.Description.Text="Relatório simplificado do financeiro." ReportOptions.LastChange="41765,5898835995" ReportOptions.VersionBuild="1" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10; ACampoFiltro : string;&#13;&#10; AFiltro : string;     &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; //showmessage(QuerGrupoPessoa.SQL.text);&#13;&#10;// showmessage('eraconta');                                                                       &#13;&#10;//  QuerConta.Close;                                                &#13;&#10;//  QuerConta.ParamByName('CodConta').Value := &#60;Financeiro.&#34;CODCONTA&#34;&#62;;  &#13;&#10;//  QuerConta.Open;&#13;&#10;                     &#13;&#10;end;&#13;&#10;&#13;&#10;function GetTotal(const ATipo: string): Double;     &#13;&#10;begin    &#13;&#10;  if &#60;Financeiro.&#34;TIPO&#34;&#62; = ATipo then&#13;&#10;    Result := &#60;Financeiro.&#34;TOTAL&#34;&#62;&#13;&#10;  else&#13;&#10;    Result := 0;  &#13;&#10;end;&#13;&#10;  &#13;&#10;function GetSaldo: Double;&#13;&#10;begin&#13;&#10;    &#13;&#10;  Result := GetTotal('RECEBIDA') + GetTotal('A RECEBER')          &#13;&#10;          - GetTotal('PAGA') - GetTotal('A PAGAR');&#13;&#10;         // showmessage('PASSOUiniciogrupo');              &#13;&#10;end;  &#13;&#10;  &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10; AFiltro, DTINICIO, DTFIM, Empresa : string;&#13;&#10; I : integer;&#13;&#10;   &#13;&#10;begin&#13;&#10;              &#13;&#10;  ACampoFiltro := '';&#13;&#10;  AFiltro      := '';&#13;&#10;  DTINICIO     := '';&#13;&#10;  DTFIM        := '';      &#13;&#10;  //Empresa&#13;&#10;    &#13;&#10;  DTINICIO := FormatDateTime('MM/DD/YYYY 00:00:00', Dt_Inicio.date);                  &#13;&#10;  DTFIM := FormatDateTime('MM/DD/YYYY 23:59:59', Dt_Fim.date);      &#13;&#10;             &#13;&#10;  if rbEmissao.Checked then&#13;&#10;  begin               &#13;&#10;    ACampoFiltro := 'EMISSAO';      &#13;&#10;    AFiltro := ' and f.emissao between :D1 and :D2';                 &#13;&#10; end else &#13;&#10; if rbVencimento.Checked then&#13;&#10;  begin             &#13;&#10;    ACampoFiltro := 'VENCTO';      &#13;&#10;    ACampoFiltro := ' and f.vencto between :D1 and :D2';                          &#13;&#10;end else&#13;&#10; if rbPagamento.Checked then&#13;&#10;   begin      &#13;&#10;    ACampoFiltro := 'DATAPAGTO';   &#13;&#10;    ACampoFiltro := ' and f.datapagto between :D1 and :D2';        &#13;&#10;end;&#13;&#10;    &#13;&#10;  if not CkRecebidas.Checked then &#13;&#10;    AFiltro := AFiltro + ' and not F.TIPO = ''RECEBIDA'' ';&#13;&#10;  if not CkPaga.Checked then &#13;&#10;    AFiltro := AFiltro + ' and not F.TIPO = ''PAGA'' ';&#13;&#10;  if not CkAReceber.Checked then &#13;&#10;    AFiltro := AFiltro + ' and not F.TIPO = ''A RECEBER'' ';&#13;&#10;  if not CkAPagar.Checked then &#13;&#10;    AFiltro := AFiltro + ' and not F.TIPO = ''A PAGAR'' ';&#13;&#10;            &#13;&#10;  GroupHeader1.Visible := RbSim.Checked;                            &#13;&#10;  GroupFooter1.Visible := RbSim.Checked;&#13;&#10;                        &#13;&#10;  if rbCredSim.Checked then MasterData1.Filter := '';      &#13;&#10;  if rbCredNao.Checked then MasterData1.Filter := '&#60;Financeiro.&#34;TIPOPAGTO&#34;&#62; &#60;&#62; ''CREDIÁRIO''';&#13;&#10;               &#13;&#10;  QuerGrupoPessoa.close;&#13;&#10;  QuerGrupoPessoa.SQL.Add(AFiltro);                        &#13;&#10;  QuerGrupoPessoa.ParamByName('codigo_grupo').Value := DBLookupComboBox1.KeyValue;&#13;&#10;  QuerGrupoPessoa.ParamByName('D1').Value := WR2StartOfTheDay(Dt_Inicio.Date);              &#13;&#10;  QuerGrupoPessoa.ParamByName('D2').Value := WR2EndOfTheDay(Dt_Fim.Date);                    &#13;&#10;  showmessage(QuerGrupoPessoa.SQL.text);            &#13;&#10;  QuerGrupoPessoa.open;&#13;&#10; //showmessage('deuok');      &#13;&#10;end;        &#13;&#10;      &#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin    &#13;&#10;  QuerGrupo.open;&#13;&#10;  QuerGrupo.last;&#13;&#10;  QuerGrupo.first;      &#13;&#10;end;&#13;&#10;  &#13;&#10;begin       &#13;&#10; case &#60;TipoFiltroData&#62; of&#13;&#10;    0: rbEmissao.Checked    := True;                             &#13;&#10;    1: rbVencimento.Checked := True;       &#13;&#10;    2: rbPagamento.Checked  := True;           &#13;&#10;  end;&#13;&#10;         &#13;&#10;        &#13;&#10;  if &#60;dtinicioNotaFiscal&#62; &#60;&#62; '00/00/0000' then&#13;&#10;  begin             &#13;&#10;    Dt_Inicio.Date :=  StrToDate(&#60;dtinicioNotaFiscal&#62;);  &#13;&#10;    Dt_Fim.Date    :=  StrToDate(&#60;dtfimNotaFiscal&#62;);        &#13;&#10;  end;&#13;&#10;            &#13;&#10;  if &#60;dtinicioEmissao&#62; &#60;&#62; '00/00/0000' then&#13;&#10;  begin     &#13;&#10;    Dt_Inicio.Date :=  StrToDate(&#60;dtinicioEmissao&#62;);    &#13;&#10;    Dt_Fim.Date    :=  StrToDate(&#60;dtfimEmissao&#62;);        &#13;&#10;  end;&#13;&#10;              &#13;&#10;  if &#60;dtinicioVencimento&#62; &#60;&#62; '00/00/0000' then&#13;&#10;  begin        &#13;&#10;    Dt_Inicio.Date :=  StrToDate(&#60;dtinicioVencimento&#62;);  &#13;&#10;    Dt_Fim.Date    :=  StrToDate(&#60;dtfimVencimento&#62;);        &#13;&#10;  end;&#13;&#10;          &#13;&#10;  if &#60;dtinicioPagamento&#62; &#60;&#62; '00/00/0000' then&#13;&#10;  begin        &#13;&#10;    Dt_Inicio.Date :=  StrToDate(&#60;dtinicioPagamento&#62;);  &#13;&#10;    Dt_Fim.Date    :=  StrToDate(&#60;dtfimPagamento&#62;);        &#13;&#10;  end;&#13;&#10;              &#13;&#10;  if (&#60;dtinicioPagamento&#62; = '00/00/0000') and (&#60;dtinicioVencimento&#62; = '00/00/0000')                  &#13;&#10;     and (&#60;dtinicioEmissao&#62; = '00/00/0000') and (&#60;dtinicioNotaFiscal&#62; = '00/00/0000') then         &#13;&#10;  begin      &#13;&#10;    Dt_Inicio.Date := Now;             &#13;&#10;    Dt_Fim.Date    := Now;        &#13;&#10;  end;&#13;&#10;    &#13;&#10;  CkRecebidas.Checked := &#60;CkRecebida&#62;;  &#13;&#10;  CkAReceber.Checked  := &#60;CkAReceber&#62;;      &#13;&#10;  CkPaga.Checked      := &#60;CkPaga&#62;;   &#13;&#10;  CkAPagar.Checked    := &#60;CkAPagar&#62;;&#13;&#10;&#13;&#10;end.  &#13;&#10;      &#13;&#10;procedure btnCancelarOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    &#13;&#10;  DialogPage1.ModalResult     := mrCancel;&#13;&#10;  Report.PreviewOptions.Modal := False;&#13;&#10;      &#13;&#10;end;    &#13;&#10;  &#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;showmessage('INICIODESCRICAO');           &#13;&#10;  if &#60;QuerGrupo.&#34;DESCRICAO&#34;&#62; = '' then&#13;&#10;   DescricaoGrupo.Lines.Text := 'SEM GRUPO INFORMADO'             &#13;&#10;  else&#13;&#10;    DescricaoGrupo.Lines.Text := &#60;QuerGrupo.&#34;DESCRICAO&#34;&#62;;&#13;&#10;  showmessage('FIMDESCRICAO');            &#13;&#10;end;&#13;&#10;     &#13;&#10;&#13;&#10;         &#13;&#10;begin&#13;&#10;      &#13;&#10;  CarregaLogoEmpresa(Logo);  &#13;&#10;  WR2CarregaInformacoesCabecalho(MemoFantasia, MemoEndereco);  &#13;&#10;                   &#13;&#10;  EdtFiltros.Lines.Clear;&#13;&#10;  if &#60;DtInicioNotaFiscal&#62; &#60;&#62; '00/00/0000' then  &#13;&#10;    EdtFiltros.Lines.Add('Período de Nota Fiscal de [&#60;DtInicioNotaFiscal&#62;] à [&#60;DtFimNotaFiscal&#62;]');   &#13;&#10;  if &#60;DtInicioEmissao&#62; &#60;&#62; '00/00/0000' then&#13;&#10;    EdtFiltros.Lines.Add('Período de Emissão de [&#60;DtInicioEmissao&#62;] à [&#60;DtFimEmissao&#62;]');   &#13;&#10;  if &#60;DtInicioVencimento&#62; &#60;&#62; '00/00/0000' then&#13;&#10;    EdtFiltros.Lines.Add('Período de Vencimento de [&#60;DtInicioVencimento&#62;] à [&#60;DtFimVencimento&#62;]');&#13;&#10;  if &#60;DtInicioPagamento&#62; &#60;&#62; '00/00/0000' then&#13;&#10;    EdtFiltros.Lines.Add('Período de Pagamento de [&#60;DtInicioPagamento&#62;] à [&#60;DtFimPagamento&#62;]');&#13;&#10;        &#13;&#10;  //A banda ReportTitle está com Streched True, então adicionamos umas linhas vazias no final deste text para dar um espaço até o&#13;&#10;  //início da próxima banda                                            &#13;&#10;  EdtFiltros.Lines.Add('');&#13;&#10;  EdtFiltros.Lines.Add('');             &#13;&#10;  &#13;&#10;&#13;&#10;  //EdtConta.Visible := not QuerConta.IsEmpty;&#13;&#10;      &#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
