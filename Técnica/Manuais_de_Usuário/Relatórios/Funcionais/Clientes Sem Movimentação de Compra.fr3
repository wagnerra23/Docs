    <TfrxIBXQuery Name="QuerClientesSemCompra" UserName="QuerClientesSemCompra" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select *&#13;&#10;from (               &#13;&#10;select P.CODIGO, P.TIPO, P.CNPJCPF, P.RAZAOSOCIAL, C.DESCRICAO as CIDADE, P.UF, P.FONE1,&#13;&#10;       P.FONE2, P.EMAIL,&#13;&#10;       (select sum(V2.TOTAL)&#13;&#10;        from VENDA V2&#13;&#10;        where (V2.PESSOA_RESPONSAVEL_CODIGO = P.CODIGO)&#13;&#10;              and (V2.STATUS like 'ATIVO%')&#13;&#10;              and (V2.DT_FATURAMENTO between :FaturamentoInicio and :FaturamentoFim)) as TOTAL_FATURAMENTO,&#13;&#10;       (select max(V2.DT_FATURAMENTO)&#13;&#10;        from VENDA V2&#13;&#10;        where (V2.PESSOA_RESPONSAVEL_CODIGO = P.CODIGO)&#13;&#10;              and (V2.STATUS like 'ATIVO%')) as ULTIMA_COMPRA&#13;&#10;from PESSOAS P&#13;&#10;left join CIDADES C on (C.CODIGO = P.CODCIDADE)&#13;&#10;where (P.ATIVO is distinct from 'N')                                                                         &#13;&#10;      and not exists(select first 1 1&#13;&#10;                 from VENDA V&#13;&#10;                 left join VENDA_PRODUTO VP on (VP.CODVENDA = V.CODIGO)&#13;&#10;                 where (V.PESSOA_RESPONSAVEL_CODIGO = P.CODIGO)&#13;&#10;                       and (V.STATUS like 'ATIVO%')&#13;&#10;                       and (V.DT_FATURAMENTO between :CompraInicio and :CompraFim))&#13;&#10;group by 1, 2, 3, 4, 5, 6, 7, 8, 9) TAB         &#13;&#10;where (TAB.TOTAL_FATURAMENTO &#62; 0)                   " PropData=""/>
