<TfrxReport Tag="1" Version="6.9.6" DataSetName="frxDBVenda" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40882,3873022222" ReportOptions.Description.Text="[Dados]&#13;&#10;Versao=0.0.0.11&#13;&#10;Link=&#13;&#10;Observacao=&#13;&#10;Form=ImpriVendas_Periodo" ReportOptions.LastChange="41863,3642514583" ReportOptions.VersionBuild="12" ScriptLanguage="PascalScript" ScriptText.Text="function GetData(AComponente: TfrxDateEditControl): string;&#13;&#10;begin&#13;&#10;  Result := FormatDateTime('DD/MM/YYYY', AComponente.Date);                                               &#13;&#10;end;          &#13;&#10;  &#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin                                                                                                                                       &#13;&#10;  MemoFantasia.Lines.Text := &#60;Empresa.&#34;FANTASIA&#34;&#62;;&#13;&#10;  MemoEndereco.Lines.Text := &#60;Empresa.&#34;ENDERECO&#34;&#62; + ' - ' + &#60;Empresa.&#34;BAIRRO&#34;&#62; + ' - ' + &#60;Empresa.&#34;CIDADE&#34;&#62; + '/' + &#60;Empresa.&#34;UF&#34;&#62; + ' - ' + &#60;Empresa.&#34;CEP&#34;&#62; +&#13;&#10;                             'CNPJ: ' + &#60;Empresa.&#34;CNPJCPF&#34;&#62; + ' INSC: ' + &#60;Empresa.&#34;IEST&#34;&#62; + ' Fone: ' + &#60;Empresa.&#34;FONE1&#34;&#62; + ' / Fax: ' + &#60;Empresa.&#34;FAX&#34;&#62;;&#13;&#10;  CarregaLogoEmpresa(Logo);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GerarRelatorioOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if EdtDtCompraFim.Date &#60; EdtDtCompraInicio.Date then&#13;&#10;  begin&#13;&#10;    ShowMessage('A Data de Compra final está anterior a data inicial.');&#13;&#10;    Exit;                &#13;&#10;  end;&#13;&#10;&#13;&#10;  if EdtDtFaturamentoFim.Date &#60; EdtDtFaturamentoInicio.Date then&#13;&#10;  begin&#13;&#10;    ShowMessage('A Data de Faturamento final está anterior a data inicial.');&#13;&#10;    Exit;                &#13;&#10;  end;        &#13;&#10;    &#13;&#10;  {if &#60;CodProduto&#62; &#60;&#62; '' then&#13;&#10;  begin                                   &#13;&#10;    QuerClientesSemCompra.SQL.Add('and (VP.CODPRODUTO = ''' + &#60;CodProduto&#62; + ''')');&#13;&#10;    Child1.Visible := True;                                      &#13;&#10;  end;}  &#13;&#10;    &#13;&#10;  //QuerClientesSemCompra.SQL.Add('group by 1, 2, 3, 4, 5, 6, 7, 8, 9');    &#13;&#10;  //QuerClientesSemCompra.SQL.Add('having not (max(V.DT_FATURAMENTO) between :CompraInicio and :CompraFim)');&#13;&#10;                             &#13;&#10;  case CbOrdem.ItemIndex of&#13;&#10;    0: QuerClientesSemCompra.SQL.Add('order by TOTAL_FATURAMENTO desc');&#13;&#10;    1: QuerClientesSemCompra.SQL.Add('order by ULTIMA_COMPRA desc');    &#13;&#10;    2: QuerClientesSemCompra.SQL.Add('order by RAZAOSOCIAL asc');        &#13;&#10;    3: QuerClientesSemCompra.SQL.Add('order by CIDADE asc');              &#13;&#10;  end;                  &#13;&#10;&#13;&#10;  QuerClientesSemCompra.ParamByName('CompraInicio').Value      := WR2StartOfTheDay(EdtDtCompraInicio.Date);&#13;&#10;  QuerClientesSemCompra.ParamByName('CompraFim').Value         := WR2EndOfTheDay(EdtDtCompraFim.Date);                                                                                                                   &#13;&#10;  QuerClientesSemCompra.ParamByName('FaturamentoInicio').Value := WR2StartOfTheDay(EdtDtFaturamentoInicio.Date);&#13;&#10;  QuerClientesSemCompra.ParamByName('FaturamentoFim').Value    := WR2EndOfTheDay(EdtDtFaturamentoFim.Date);      &#13;&#10;&#13;&#10;  //ShowMessage(QuerClientesSemCompra.SQL.Text);                                                             &#13;&#10;  QuerClientesSemCompra.Open;&#13;&#10;  DialogPage1.ModalResult := mrOk;                                                        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  EdtDtCompraInicio.Date      := WR2IncMonth(Now, -6);&#13;&#10;  EdtDtCompraFim.Date         := &#60;Date&#62;;&#13;&#10;  EdtDtFaturamentoFim.Date    := EdtDtCompraInicio.Date;      &#13;&#10;  EdtDtFaturamentoInicio.Date := WR2IncMonth(EdtDtFaturamentoFim.Date, -6);        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure EdtDtCompraInicioOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  EdtDtFaturamentoFim.Date := EdtDtCompraInicio.Date;                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Child2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Child2.Visible := (&#60;QuerClientesSemCompra.&#34;EMAIL&#34;&#62; &#60;&#62; '');                                            &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  QuerProduto.Close;&#13;&#10;  QuerProduto.ParamByName('CODPRODUTO').Value := &#60;CodProduto&#62;;                                                                  &#13;&#10;  QuerProduto.Open;&#13;&#10;  WR2CarregaInformacoesCabecalho(MemoFantasia, MemoEndereco);      &#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
