<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="44735,7242189468" ReportOptions.Description.Text="" ReportOptions.LastChange="44735,7242189468" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  Date1 : String;&#13;&#10;  Date2 : String;&#13;&#10;  ASQL  : String;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin   &#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo7OnAfterData(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if Memo7.value &#60;= 0 then&#13;&#10;     Memo7.Font.Color := clRed&#13;&#10;  else&#13;&#10;     Memo7.Font.Color := clBlue  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnCancelarOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  DialogPage1.ModalResult     := mrCancel;&#13;&#10;  Report.PreviewOptions.Modal := False;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnConfirmarOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ASQL := Produto.SQL.Text;    &#13;&#10;  // FILTRO DATAS  &#13;&#10;  Date1 := FormatDateTime('DD.MM.YYYY', &#60;DtInicio&#62;);&#13;&#10;  Date2 := FormatDateTime('DD.MM.YYYY 23:59:59', &#60;DtFim&#62;);           &#13;&#10;  case &#60;TipoFiltroData&#62; of              &#13;&#10;    0 : ASQL := ASQL + 'where  (V.DT_EMISSAO between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) and vp.parent is null and (V.is_venda = ''S'') and(V.ativo = ''S'') and';&#13;&#10;    1 : ASQL := ASQL + 'where  (V.dt_faturamento between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) and vp.parent is null and (V.is_venda = ''S'') and(V.ativo = ''S'') and';&#13;&#10;  end;&#13;&#10;  case &#60;TipoSelecao&#62; of              &#13;&#10;    0 : ASQL := ASQL + ' not (v.dt_faturamento is null) and (v.status like ''ATIVO%'') ';                               &#13;&#10;    1 : ASQL := ASQL + '(v.dt_faturamento is null) and (v.status like ''ATIVO%'') ';                            &#13;&#10;    2 : ASQL := ASQL + '(v.status = ''INATIVO'') ';                                 &#13;&#10;    3 : ASQL := ASQL + '(v.status like ''ATIVO%'') ';                               &#13;&#10;  end;          &#13;&#10;  case &#60;CodEmpresa&#62; of&#13;&#10;    1 : ASQL := ASQL + 'and RIGHT(V.codigo,1) = '''+&#60;CodEmpresa&#62;+''' ';                                                                                                         &#13;&#10;    2 : ASQL := ASQL + 'and RIGHT(V.codigo,2) = '''+&#60;CodEmpresa&#62;+''' ';&#13;&#10;    3 : ASQL := ASQL + 'and V.codigo like ''%-3%''';&#13;&#10;    4 : ASQL := ASQL + 'and V.codigo like ''%-4%'''        &#13;&#10;  end;&#13;&#10;  ASQL := ASQL + 'and ((PG.CODNF_NATUREZA_OPERACAO is null) or (PG.CODNF_NATUREZA_OPERACAO = 0)) ';&#13;&#10;&#13;&#10;  if rbCategoria.Checked = true then ASQL := ASQL + ' group by 1, 2, 3, 4, 5, 6, 7 order by CATEGORIA, P.DESCRICAO;';&#13;&#10;  &#13;&#10;  if rbGrupodeproduto.Checked = true then&#13;&#10;  begin              &#13;&#10;    ASQL := ASQL + ' group by 1, 2, 3, 4, 5, 6, 7 order by GRUPO, P.DESCRICAO;';&#13;&#10;    GroupHeader1.Condition := 'Produto.&#34;GRUPO&#34;';        &#13;&#10;    if &#60;Produto.&#34;GRUPO&#34;&#62; &#60;&#62; '' then&#13;&#10;    begin                &#13;&#10;      Memo21.Text := 'GRUPO: [Produto.&#34;GRUPO&#34;]';&#13;&#10;    end else&#13;&#10;      Memo21.Text := 'SEM GRUPO';                &#13;&#10;  end;                  &#13;&#10;                &#13;&#10;  Produto.SQL.Text := ASQL;      &#13;&#10;  ShowMessage(ASQL);&#13;&#10;  DialogPage1.ModalResult := mrOk        &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  CarregaLogoEmpresa(Logo);                      &#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
