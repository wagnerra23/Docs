    <TfrxIBXQuery Name="Produto" UserName="Produto" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select v.CODIGO, P.DESCRICAO, P.CODPRODUTO_TIPO as TIPO, P.CODPRODUTO_GRUPO, PG.DESCRICAO as GRUPO, PC.DESCRICAO as CATEGORIA,&#13;&#10;       VP.CODPRODUTO, count(VP.CODPRODUTO) as COUNTI, sum(coalesce(VP.VDESC, 0)) as DESCONTO,&#13;&#10;       sum(coalesce(VP.VOUTRO, 0)) as ACRESCIMO, sum(coalesce(VP.NF_VFRETE, 0)) as FRETE,&#13;&#10;       sum(coalesce(VP.VALOR, 0) * coalesce(VP.QUANT, 0)) as SUBTOTALSOMA,&#13;&#10;       sum(coalesce(VP.VOUTRO, 0) - coalesce(VP.VDESC, 0)) as JUROSACRECIMO,&#13;&#10;       sum(coalesce(VP.custo, 0) * coalesce(VP.QUANT, 0)) as TOTALCUSTO,             &#13;&#10;       sum(coalesce(VP.TOTAL_RELATORIO, 0)) as RESULTADOSOMA, sum(coalesce(VP.QUANT, 0)) as QTD&#13;&#10;from VENDA_PRODUTO VP&#13;&#10;left join VENDA V on (V.CODIGO = VP.CODVENDA)&#13;&#10;left join PRODUTO P on (VP.CODPRODUTO = P.CODIGO)&#13;&#10;left join PRODUTO_GRUPO PG on (PG.CODIGO = P.CODPRODUTO_GRUPO)&#13;&#10;left join PRODUTO_CATEGORIA PC on (PC.CODIGO = P.CODPRODUTO_CATEGORIA)  " PropData="05704C65667403AF000470546F700384000A506172616D65746572730100"/>
