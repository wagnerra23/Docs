<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="PadrÃ£o" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="43833,729951088" ReportOptions.Description.Text="[Dados]&#13;&#10;Versao=0.0.0.0&#13;&#10;Link=&#13;&#10;Observacao=&#13;&#10;Form=TImpriVendas_Periodo" ReportOptions.LastChange="43833,729951088" ScriptLanguage="PascalScript" ScriptText.Text="procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  //Pintar as linhas&#13;&#10;  if (MasterData1.DataSet.RecNo mod 2) = 0 then              &#13;&#10;    TfrxBrushFill(MasterData1.Fill).BackColor := cl3DLight               &#13;&#10;  else                &#13;&#10;    TfrxBrushFill(MasterData1.Fill).BackColor := clNone;            &#13;&#10;end;&#13;&#10;                               &#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin   &#13;&#10;  //ShowMessage(QuerEstoque.SQL.Text);                                                    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  categoria : String;                                                                                        &#13;&#10;begin&#13;&#10;  //carregalogoempresa(Picture1);  &#13;&#10;&#13;&#10;  QuerEstoque.SQL.Text := 'select P.CODIGO, P.DESCRICAO, coalesce(P.CUSTO, 0) as CUSTO_LOJA, coalesce(PE.ESTOQUE, 0) as ESTOQUE, P.unidade, '+       &#13;&#10;      ' P.CUSTO as CUSTO_TOTAL, '+       &#13;&#10;      ' P.VALOR as VALOR, '+       &#13;&#10;      '((coalesce(P.VALOR, 0) - coalesce(P.CUSTO, 0)) * PE.ESTOQUE) as LUCRO '+                              &#13;&#10;      'from PRODUTO P '+       &#13;&#10;      'left join PRODUTO_ESTOQUE PE on (P.CODIGO = PE.CODPRODUTO) '+&#13;&#10;      'left join PRODUTO_CATEGORIA PC on (P.CODPRODUTO_CATEGORIA = PC.CODIGO)'+                                                                                                                                                                                                          &#13;&#10;      'where P.ATIVO = ''S'' ';&#13;&#10;&#13;&#10;  if &#60;CodEmpresa&#62; &#60;&#62; '0' then&#13;&#10;  begin                                                           &#13;&#10;    QuerEstoque.SQL.Add(' and PE.CODEMPRESA = ''' + &#60;CodEmpresa&#62; + '''') ;&#13;&#10;    EmpresaRel.SQL[3] := 'WHERE E.CODIGO = ''' + &#60;CodEmpresa&#62; + ''' ';        &#13;&#10;    CarregaLogoEmpresaRelatorio(ImgLogo,&#60;CodEmpresa&#62;);  &#13;&#10;    MbEmpresa.Lines.Text := &#60;EmpresaRel.&#34;FANTASIA&#34;&#62;;                             &#13;&#10;    MemoFantasia.Text := &#60;EmpresaRel.&#34;FANTASIA&#34;&#62;;           &#13;&#10;    MemoEndereco.Lines.Text := &#60;EmpresaRel.&#34;ENDERECO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;BAIRRO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;CIDADE&#34;&#62; + '/' + &#60;EmpresaRel.&#34;UF&#34;&#62; + #13#10 +      &#13;&#10;                             'CEP: ' + &#60;EmpresaRel.&#34;CEP&#34;&#62; +  ' CNPJ: ' + &#60;EmpresaRel.&#34;CNPJCPF&#34;&#62; + ' INSC: ' + &#60;EmpresaRel.&#34;IEST&#34;&#62; + #13#10 +&#13;&#10;                             'Fone: ' + &#60;EmpresaRel.&#34;FONE1&#34;&#62; + ' / Fax: ' + &#60;EmpresaRel.&#34;FAX&#34;&#62;;             &#13;&#10;  end else&#13;&#10;  begin              &#13;&#10;    MbEmpresa.Lines.Text := 'Todas as Empresas';      &#13;&#10;    //carregalogoempresa(ImgLogo);      &#13;&#10;  end;                                                                                      &#13;&#10;                                        &#13;&#10;  if VarToStr(&#60;CodProduto&#62;) &#60;&#62; '' then&#13;&#10;  begin                                                             &#13;&#10;    QuerEstoque.SQL.Add('and p.codigo =  :CodProduto');&#13;&#10;    Memo15.Text := &#60;CodProduto&#62;+' - '+&#60;PRODUTO&#62;;&#13;&#10;    Memo15.Font.Color := 8388608;                                    &#13;&#10;  end;&#13;&#10;&#13;&#10;  if CBZerado.Checked = True Then&#13;&#10;  begin&#13;&#10;    QuerEstoque.SQL.Add('and PE.ESTOQUE &#60;&#62; 0 ');    &#13;&#10;  end;&#13;&#10;&#13;&#10;  if CBNegativo.Checked = True Then&#13;&#10;  begin&#13;&#10;    QuerEstoque.SQL.Add('and pe.ESTOQUE &#62;= 0 ');    &#13;&#10;  end;              &#13;&#10;&#13;&#10;  if cbCategoria.Text &#60;&#62; 'Todos' then&#13;&#10;  begin  &#13;&#10;    categoria := cbCategoria.text;                                                                           &#13;&#10;    QuerEstoque.SQL.Add('and PC.DESCRICAO = ''' + categoria + ''' ');&#13;&#10;    Memo38.Text := cbCategoria.Text;                                                                                          &#13;&#10;  end;            &#13;&#10;&#13;&#10;  QuerEstoque.SQL.Add( 'order by PE.ESTOQUE desc,p.descricao');&#13;&#10;  &#13;&#10;  if VarToStr(&#60;CodProduto&#62;) &#60;&#62; '' then&#13;&#10;  Begin              &#13;&#10;    QuerEstoque.ParambyName('CodProduto').value := &#60;CodProduto&#62;;&#13;&#10;  End;            &#13;&#10;  QuerEstoque.Open;      &#13;&#10;  //ShowMessage(WR2GetListaVariaveis);                &#13;&#10;end;  &#13;&#10;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  while not querCategoria.Eof do&#13;&#10;  begin              &#13;&#10;    cbCategoria.Items.AddObject(&#60;querCategoria.&#34;DESCRICAO&#34;&#62;, nil);&#13;&#10;    querCategoria.Next;&#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;       &#13;&#10;end." StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
