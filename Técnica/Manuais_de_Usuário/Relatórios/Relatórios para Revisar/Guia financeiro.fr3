    <TfrxIBXQuery Name="ProdutoVenda" UserName="ProdutoVenda" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select VP.*,&#13;&#10;PR.SITUACAO,&#13;&#10;  iif(not(VPO.CODIGO is null), VP.TOTAL - VPO.TOTAL, 0) as DIFERENCA_TOTAL_VENDA_ORIGINAL,&#13;&#10;  iif(not(VPO.CODIGO is null), VP.VALOR - VPO.VALOR, 0) as DIFERENCA_VALOR_VENDA_ORIGINAL,&#13;&#10;  iif(not(VPO.CODIGO is null), VP.QUANT - VPO.QUANT, 0) as DIFERENCA_QUANT_VENDA_ORIGINAL&#13;&#10;from VENDA_PRODUTO VP&#13;&#10;left join PRODUTO P on (VP.CODPRODUTO = P.CODIGO)&#13;&#10;left join PRODUCAO PR on (VP.CODPRODUCAO = PR.CODIGO)&#13;&#10;left join VENDA_PRODUTO VPO on (VP.CODVENDA_ORIGINAL = VPO.CODVENDA) and (VP.CODVENDA_PRODUTO_ORIGINAL = VPO.CODIGO)&#13;&#10;where (VP.CODVENDA = :CODIGO) and VP.PARENT IS NULL&#13;&#10;order by sequencia                                        " PropData="05704C656674026C0470546F7002440A506172616D657465727301010623204E616D653D22434F4449474F222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxIBXQuery Name="QuerImagem" UserName="QuerImagem" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select VP.SEQUENCIA ,VP.PATH, pp.codfamilia&#13;&#10;from VENDA_PRODUTO vp&#13;&#10;left join VENDA V on (V.CODIGO = VP.CODVENDA)&#13;&#10;left join produto pp on pp.codigo = vp.codproduto&#13;&#10;where (v.codigo =:CODVENDA) and vp.codigo = :codvp                                                                                        " PropData="05704C65667403EC000470546F7002400A506172616D657465727301010625204E616D653D22434F4456454E4441222044617461547970653D226674556E6B6E6F776E2200010622204E616D653D22636F647670222044617461547970653D226674556E6B6E6F776E220000"/>
