 &#60;ProdutoVenda.&#34;Codvenda&#34;&#62;;&#13;&#10;  QuerImagem.ParamByName('codvp').value := &#60;ProdutoVenda.&#34;Codigo&#34;&#62;;&#13;&#10;  QuerImagem.open;      &#13;&#10;  Picture1.Picture := nil;        &#13;&#10;  if &#60;ProdutoVenda.&#34;PATH&#34;&#62; &#60;&#62; '' then                 &#13;&#10;    path := &#60;ProdutoVenda.&#34;PATH&#34;&#62;        &#13;&#10;  else        &#13;&#10;    path := 'C:\WR Sistema\Fotos\' + inttostr(QuerImagem.FieldByName('codfamilia').asInteger) + '-0.jpg';      &#13;&#10;&#13;&#10;  if ArquivoExiste(path) then&#13;&#10;    Picture1.LoadFromFile(path);                      &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;            &#13;&#10;  CarregaLogoEmpresa(ImgLogo);&#13;&#10;  WR2CarregaInformacoesCabecalho(MemoFantasia, MemoCabecalho);&#13;&#10;&#13;&#10;    &#13;&#10;end.&#13;&#10; " StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
