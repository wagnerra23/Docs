<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="43294,6883466435" ReportOptions.Description.Text="[Dados]&#13;&#10;Versao=0.0.0.1&#13;&#10;Link=&#13;&#10;Observacao=Observacao=Observacao=Relatório para contagem de estoque por Categoria&#13;&#10;Form=TFrmPrincipal&#13;&#10;Form=TFrmPrincipal&#13;&#10;" ReportOptions.LastChange="43294,6883466435" ReportOptions.VersionBuild="1" ScriptLanguage="PascalScript" ScriptText.Text="function WRMax(const AValue1, AValue2: Double): Double;&#13;&#10;begin&#13;&#10;  if AValue1 &#62; AValue2 then&#13;&#10;    Result := AValue1  &#13;&#10;  else  &#13;&#10;    Result := AValue2;&#13;&#10;end;&#13;&#10;  &#13;&#10;var&#13;&#10;  ASQL : string;&#13;&#10;  margemMinima : Integer;                                                                                  &#13;&#10;&#13;&#10;procedure ColoreCampos(Color : Integer);&#13;&#10;begin&#13;&#10;  memoValor.Color := Color;&#13;&#10;  memoMargem.Color := Color;        &#13;&#10;end;        &#13;&#10;&#13;&#10;procedure carregaComboBox(dataset : TfrxIBXQuery);        &#13;&#10;begin&#13;&#10;  dataset.Open;&#13;&#10;  dataset.Last;  &#13;&#10;  dataset.First;        &#13;&#10;end;          &#13;&#10;         &#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  carregalogoempresa(Picture1);&#13;&#10;  margemPadraoPorEmpresa.open;                  &#13;&#10;  margemMinima := margemPadraoPorEmpresa.FieldByName('MARGEM_PADRAO').Value - 1;                                            &#13;&#10;  &#13;&#10;  //Filtro de Grupo de Produtos                                                                  &#13;&#10; { if &#60;CodProdutoGrupo&#62; &#60;&#62; '' then      &#13;&#10;  begin&#13;&#10;    QuerProduto.SQL[6] := 'and p.codproduto_grupo = :codproduto_grupo';&#13;&#10;    QuerProduto.ParamByName('codproduto_grupo').Value := &#60;CodProdutoGrupo&#62;;&#13;&#10;    Memo36.Lines.Text := &#60;CodProdutoGrupo&#62; +' - [QuerProduto.&#34;GRUPO&#34;]';   &#13;&#10;    Memo36.Font.Color := 8388608;             &#13;&#10;  end;             }  &#13;&#10;    &#13;&#10;   //Filtro de Produtos        &#13;&#10; { if &#60;CodProduto&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    QuerProduto.SQL[7] := 'and p.codigo = :codigo';&#13;&#10;    QuerProduto.ParamByName('codigo').Value := &#60;CodProduto&#62;;&#13;&#10;    Memo43.Lines.Text := &#60;CodProduto&#62; +' - [Venda_Produto.&#34;DESCRICAO_PRODUTO&#34;]';&#13;&#10;    Memo43.Font.Color := 8388608;           &#13;&#10;  end; }             &#13;&#10;&#13;&#10;    //ORDER BY&#13;&#10; { case &#60;tipoordenacao&#62; of             &#13;&#10;    0 : &#13;&#10;    begin            &#13;&#10;      QuerProduto.SQL[20] := 'order by Categoria desc,P.DESCRICAO';&#13;&#10;      Mbordenado.Lines.Text :=  'Descrição do Produto';&#13;&#10;      Mbordenado.Font.Color := 8388608;                                            &#13;&#10;    end;                      &#13;&#10;    1 : &#13;&#10;    begin&#13;&#10;      QuerProduto.SQL[20] := 'order by Categoria desc,P.codigo';&#13;&#10;      Mbordenado.Lines.Text :=  'Código do Produto';&#13;&#10;      Mbordenado.Font.Color := 8388608;                    &#13;&#10;    end;          &#13;&#10;    2 : &#13;&#10;    begin&#13;&#10;      QuerProduto.SQL[20] := ' order by Categoria desc,PC.DESCRICAO desc';&#13;&#10;      Mbordenado.Lines.Text :=  'Categoria';&#13;&#10;      Mbordenado.Font.Color := 8388608;          &#13;&#10;    end;           &#13;&#10;  end;}  &#13;&#10;        &#13;&#10;  QuerProduto.SQL[20] := ' order by Categoria desc,P.DESCRICAO asc';&#13;&#10;  Mbordenado.Lines.Text :=  'Categoria';&#13;&#10;  Mbordenado.Font.Color := 8388608;           &#13;&#10; // ShowMessage(QuerProduto.SQL.Text);                                        &#13;&#10;  QuerProduto.ParamByName('empresacodigo').value := cbxEmpresa.Keyvalue;                                                            &#13;&#10;  QuerProduto.Open;&#13;&#10;  EmpresaSelecionada.Close;                    &#13;&#10;  EmpresaSelecionada.ParamByName('codEmpresaSelecionada').value := cbxEmpresa.Keyvalue;&#13;&#10;  EmpresaSelecionada.Open;   &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  //Pintar as linhas&#13;&#10;  if (MasterData1.DataSet.RecNo mod 2) = 0 then              &#13;&#10;    TfrxBrushFill(MasterData1.Fill).BackColor := cl3DLight             &#13;&#10;  else                &#13;&#10;    TfrxBrushFill(MasterData1.Fill).BackColor := clNone;        &#13;&#10;    &#13;&#10;  QuerProduto.Open;&#13;&#10;    &#13;&#10;  if (QuerProduto.FieldByName('margem').value &#60; margemMinima) and (QuerProduto.FieldByName('CUSTO').value &#62; 0) then&#13;&#10;  begin              &#13;&#10;    ColoreCampos($8080F0);&#13;&#10;    setaVermelha.Visible := True;&#13;&#10;    setaVerde.Visible := False;                                                              &#13;&#10;  end else begin                 &#13;&#10;    ColoreCampos(clNone);&#13;&#10;    setaVermelha.Visible := False;&#13;&#10;    setaVerde.Visible := True;    &#13;&#10;  end;            &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure MasterData1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  setaVerde.Height := MasterData1.Height;&#13;&#10;  setaVermelha.Height := MasterData1.Height;      &#13;&#10;end;    &#13;&#10;&#13;&#10;procedure BitBtn1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;  if VarToStr(DBLookupComboBox2.Keyvalue) &#60;&#62; '' then&#13;&#10;  begin                      &#13;&#10;    QuerProduto.SQL[12] := 'and pt.descricao = '+''''+DBLookupComboBox2.Keyvalue+'''';                 &#13;&#10;    //QuerProduto.SQL[12] := 'and pt.descricao = '+''''+ComboBox1.Items.Strings[ComboBox1.itemindex]+'''';&#13;&#10;    //ShowMessage(QuerProduto.SQL[12]);      &#13;&#10;  end;            &#13;&#10;&#13;&#10;  if VarToStr(DBLookupComboBox1.Keyvalue) &#60;&#62; '' then&#13;&#10;  begin              &#13;&#10;    QuerProduto.SQL[13] := 'and pc.descricao = '+''''+DBLookupComboBox1.Keyvalue+'''';                                &#13;&#10;    //ShowMessage(QuerProduto.SQL[13]);    &#13;&#10;  end;&#13;&#10;  if cbxSocomEstoque.Checked then&#13;&#10;  begin              &#13;&#10;    QuerProduto.SQL[14] := ' and pe.estoque &#62; 0 ';                                &#13;&#10;    //ShowMessage(QuerProduto.SQL[13]);    &#13;&#10;  end;      &#13;&#10;&#13;&#10; // ShowMessage( QuerProduto.SQL);                                            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;QuerProduto.&#34;CATEGORIA&#34;&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    QuerProdutoCATEGORIA1.Visible := True;                                                     &#13;&#10;    Memo3.Visible := False;                                                            &#13;&#10;  end else&#13;&#10;  begin&#13;&#10;    QuerProdutoCATEGORIA1.Visible := False;                                                     &#13;&#10;    Memo3.Visible := True;             &#13;&#10;  end;              &#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  showMessage(Empresas.ClassName);                                    &#13;&#10;  carregaComboBox(Empresas);&#13;&#10;  carregaComboBox(QuerTipoProduto);&#13;&#10;  carregaComboBox(QuerProdutoCategoria);  &#13;&#10;  cbxEmpresa.Keyvalue := 1;   &#13;&#10;end;  &#13;&#10;&#13;&#10;begin&#13;&#10;      &#13;&#10;end.          " StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
