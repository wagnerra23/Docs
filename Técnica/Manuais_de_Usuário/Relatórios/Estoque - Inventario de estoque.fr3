    <TfrxIBXQuery Name="Venda_Produto" UserName="Venda_Produto" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select PC.DESCRICAO as CATEGORIA, VP.CODPRODUTO as CODIGO_PRODUTO, P.DESCRICAO as DESCRICAO_PRODUTO,&#13;&#10;       sum(VP.QUANT) as QTDE, sum(VP.CUSTO_VENDA) as CUSTO_VENDA, sum(VP.TOTAL) as TOTAL,&#13;&#10;       sum(VP.ACRESCIMO_DESCONTO) as ACRESCIMO_DESCONTO, sum(VP.ACRESCIMO) as ACRESCIMO, sum(VP.SUB_TOTAL) as SUBTOTAL,&#13;&#10;       sum(((VP.TOTAL - VP.ACRESCIMO_DESCONTO) + VP.ACRESCIMO)) as VALOR_COM_DESCONTO&#13;&#10;&#13;&#10;from VENDA_PRODUTO as VP&#13;&#10;&#13;&#10;left join VENDA as V on (V.CODIGO = VP.CODVENDA)                                                 &#13;&#10;&#13;&#10;left join PESSOAS F on (F.CODIGO = V.PESSOA_FUNCIONARIO_CODIGO)&#13;&#10;&#13;&#10;left join PESSOAS R on (R.CODIGO = V.PESSOA_REPRESENTANTE_CODIGO)&#13;&#10;&#13;&#10;left join PRODUTO P on (P.CODIGO = VP.CODPRODUTO)&#13;&#10;&#13;&#10;left join PRODUTO_CATEGORIA PC on (PC.CODIGO = P.CODPRODUTO_CATEGORIA)&#13;&#10;  &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;   &#13;&#10;               &#13;&#10;&#13;&#10;&#13;&#10;  &#13;&#10;group by PC.DESCRICAO, VP.CODPRODUTO&#13;&#10;    &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;                              " PropData="05704C65667403A5000470546F7002340A506172616D65746572730100"/>
