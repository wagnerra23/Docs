<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Modal="False" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42145,6095804398" ReportOptions.Description.Text="RELATORIO DE VENDAS" ReportOptions.LastChange="42145,6095804398" ReportOptions.VersionBuild="14" ReportOptions.VersionMajor="2023" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="1" ScriptLanguage="PascalScript" ScriptText.Text="/////////APLICA O SQL INICIAL E NECESSARIO DA VENDA\\\\\\\\\\\\\\\\\\\\\\&#13;&#10;const&#13;&#10;  ASQLSelVenda_Produto = ' select VP.CODPRODUTO AS CODPRODUTO, vp.margem, VP.VALOR, vp.quant, VP.TOTAL_IMPOSTOS, VP.descricao, ' +&#13;&#10;          ' VP.vdesc,  VP.VOUTRO, VP.CUSTO, VP.total, VP.COMP, VP.UNIDADE, VP.QTDADEPECA, VP.LARG, VP.ESPESSURA, ' +&#13;&#10;          ' vp.total_relatorio, vp.calc_valor_original, vp.calc_valor_tabela_preco, vp.calc_vdesc_manual, ' + &#13;&#10;          ' vp.calc_voutro_manual, vp.nf_ipi_vipi, vp.nf_iss_viss, vp.calc_tipo_desconto,' +&#13;&#10;          ' vp.NF_VFRETE, vp.NF_VICMSST, vp.NF_VALOR_ISS_RETIDO,' +              &#13;&#10;          'iif(vp.calc_tipo_desconto = ''Desconto pela Tabela de preço'', vp.CALC_VDESC_MANUAL, 0) AS DESCONTO_TABELA_PRECO,  '  +    &#13;&#10;          'iif(vp.calc_tipo_desconto = ''Acréscimo pela Tabela de preço'', vp.CALC_VOUTRO_MANUAL, 0) AS ACRESCIMO_TABELA_PRECO, ' +    &#13;&#10;          'iif(vp.calc_tipo_desconto = ''Acréscimo R$'', vp.CALC_VOUTRO_MANUAL, 0) AS ACRESCIMO_MANUAL, ' +    &#13;&#10;          'iif(vp.calc_tipo_desconto = ''Desconto R$'', vp.CALC_VDESC_MANUAL, 0) AS DESCONTO_MANUAL, ' +                    &#13;&#10;          ' VP.CALC_VLUCRO, VP.DT_VALOR_ORIGINAL, TP.DESCRICAO AS DESCRICAO_TABELA, V.PESSOA_RESPONSAVEL_CODIGO, ' +                                                     &#13;&#10;          ' V.CODIGO, V.RAZAOSOCIAL, V.DT_EMISSAO, V.NOTAFISCAL, V.VENDA_TIPO, ' +     &#13;&#10;          ' V.SITUACAOFINANCEIRA, V.TELEFONE, V.pessoa_representante_codigo, UD.USUARIO AS USUARIO_DESCONTO, V.CONDICAOPAGTO,' + &#13;&#10;          ' PT.DESCRICAO AS PRODUTO_TIPO, PG.DESCRICAO AS PRODUTO_GRUPO, PF.RAZAOSOCIAL AS P_FUNCIONARIO, PR2.RAZAOSOCIAL AS P_REPRESENTANTE ';&#13;&#10;Var&#13;&#10;  ACampoAgrupadorGrafico:String;&#13;&#10;  MasterData1Height, MasterData2Height: double;                                                                                        &#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AArqINI : TIniFile;                                                            &#13;&#10;begin&#13;&#10;  AArqINI := TIniFile.create(ExtractFilePath(Application.ExeName) + '\Relatorio.ini');&#13;&#10;  try&#13;&#10;    rbCliente.Checked            := AArqINI.ReadBool('VENDAS', 'rbCliente',True);                 &#13;&#10;    RbFuncionario.Checked        := AArqINI.ReadBool('VENDAS', 'RbFuncionario',False);&#13;&#10;    RbRepresentante.Checked      := AArqINI.ReadBool('VENDAS', 'RbRepresentante',False);  &#13;&#10;    RbAgencia.Checked            := AArqINI.ReadBool('VENDAS', 'RbAgencia',False);  &#13;&#10;    RbUsuario.Checked            := AArqINI.ReadBool('VENDAS', 'RbUsuario',False);  &#13;&#10;    RbTipovenda.Checked          := AArqINI.ReadBool('VENDAS', 'RbTipovenda',False);                                             &#13;&#10;    RbGrupoCliente.Checked       := AArqINI.ReadBool('VENDAS', 'RbGrupoCliente',True);                 &#13;&#10;    RbProduto.Checked            := AArqINI.ReadBool('VENDAS', 'RbProduto',False);&#13;&#10;    RbGrupoProduto.Checked       := AArqINI.ReadBool('VENDAS', 'RbGrupoProduto',False);  &#13;&#10;    RbCidade.Checked             := AArqINI.ReadBool('VENDAS', 'RbCidade',False);  &#13;&#10;    RbEstado.Checked             := AArqINI.ReadBool('VENDAS', 'RbEstado',False);&#13;&#10;    RbSituacaoFinanceira.Checked := AArqINI.ReadBool('VENDAS', 'RbSituacaoFinanceira',True);                 &#13;&#10;    Rbnaoagrupar.Checked         := AArqINI.ReadBool('VENDAS', 'Rbnaoagrupar',False);&#13;&#10;    RbCondicaoPG.Checked         := AArqINI.ReadBool('VENDAS', 'RbCondicaoPG',False);        &#13;&#10;  finally&#13;&#10;    AArqINI.Free;                    &#13;&#10;  end;                                                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnConfirmarOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AArqINI : TIniFile;        &#13;&#10;  ASQLSelVenda :String;&#13;&#10;begin&#13;&#10;  btnConfirmar.Enabled:= False;                                                &#13;&#10;  btnConfirmar.Caption:= 'Gerando...';                                                &#13;&#10;           &#13;&#10;  CarregaLogoEmpresa(Logo);                                                &#13;&#10;  AArqINI := TIniFile.create(ExtractFilePath(Application.ExeName) + '\Relatorio.ini');   // Salva última configuração, isso é muito bom                                                                                                              &#13;&#10;  try&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCliente',           RbCliente.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbFuncionario',       RbFuncionario.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbAgencia',           RbAgencia.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbRepresentante',     RbRepresentante.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbUsuario',           RbUsuario.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbTipovenda',         RbTipovenda.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbGrupoCliente',      RbGrupoCliente.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbProduto',           RbProduto.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbGrupoProduto',      RbGrupoProduto.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCidade',            RbCidade.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbEstado',            RbEstado.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbSituacaoFinanceira',RbSituacaoFinanceira.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'Rbnaoagrupar',        Rbnaoagrupar.Checked);&#13;&#10;    AArqINI.WriteBool('VENDAS', 'RbCondicaoPG',        RbCondicaoPG.Checked);              &#13;&#10;  finally&#13;&#10;    AArqINI.Free;                    &#13;&#10;  end;&#13;&#10;&#13;&#10; //////////////APLICA OS LEFT JOINS NECESSARIOS\\\\\\\\\\\\\\\\\\\\\\\\\                                                                                                                                                       &#13;&#10;&#13;&#10;  if (Rbnaoagrupar.Checked) then             &#13;&#10;    Venda_Produto.SQL.Text:= SQL_Nao_Agrupar;            &#13;&#10;&#13;&#10;  if (RbCliente.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Cliente;       &#13;&#10;&#13;&#10;  if (VarToStr(&#60;CodClienteGrupo&#62;) &#60;&#62; '') or (RbGrupoCliente.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Cliente_Grupo;&#13;&#10;&#13;&#10;  if (VarToStr(&#60;CodMarca&#62;) &#60;&#62; '') then      &#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Produto_Marca;&#13;&#10;            &#13;&#10;  if (VarToStr(&#60;CodProdutoGrupo&#62;) &#60;&#62; '') or (RbGrupoProduto.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Produto_OU_ProdutoGrupo;&#13;&#10;&#13;&#10;  if (VarToStr(&#60;CodCidade&#62;) &#60;&#62; '') or (RbCidade.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Cidade_OU_CodCidade;&#13;&#10;            &#13;&#10;  if (VarToStr(&#60;CodProduto&#62;) &#60;&#62; '') or (RbProduto.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Produto;&#13;&#10;&#13;&#10;  if (rcCategoriaProduto.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Produto_Categoria;&#13;&#10;&#13;&#10;  if (rbProdutoMarcas.Checked) then  &#13;&#10;    Venda_Produto.SQL.Text:= SQL_Por_Produto_Marca;      &#13;&#10;  &#13;&#10;  if (rbcondicaopg.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Condicao_Pagamento;&#13;&#10;      &#13;&#10;  if (RbTipovenda.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Venda_Tipo;&#13;&#10;  &#13;&#10;  if RbSituacaoFinanceira.checked = true then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Situacao_Financeira;        &#13;&#10;  &#13;&#10;  if (RbEstado.Checked = true) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Estado;&#13;&#10;      &#13;&#10;{  if (VarToStr(&#60;CodFornecedor&#62;) &#60;&#62; '') then&#13;&#10;    Venda.SQL.Text:= SQL_Por_Fornecedor;   }          &#13;&#10;  &#13;&#10;  if (RbRepresentante.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Representante;      &#13;&#10;  &#13;&#10;  if (rbfuncionario.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Funcionario;&#13;&#10;      &#13;&#10;  if (rbAgencia.Checked) then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Agencia;        &#13;&#10;                      &#13;&#10;  if (RbUsuario.checked) or (VarToStr(&#60;CodUsuario&#62;) &#60;&#62; '') then&#13;&#10;    Venda_Produto.SQL.Text:= SQL_Usuario;      &#13;&#10;    &#13;&#10;  /////////////////// FIM DOS SQLs NECESSARIOS \\\\\\\\\\\\\\\\\\\\\\\&#13;&#10;&#13;&#10;&#13;&#10;  MasterData_Grafico.Visible   := cktop10.Checked;&#13;&#10;    &#13;&#10;  DetailData_Financiero.Visible:= ckDetalhaFinanceiro.Checked;&#13;&#10;  Header_Financeiro.Visible    := ckDetalhaFinanceiro.Checked;&#13;&#10;  Footer_Financeiro.Visible    := ckDetalhaFinanceiro.Checked;        &#13;&#10;    &#13;&#10;  DetailData_Produto.Visible   := ckDetalhaProduto.Checked;&#13;&#10; // Header_DetalheProduto.Visible:= ckDetalhaProduto.Checked;&#13;&#10;  Footer_DetalheProduto.Visible:= ckDetalhaProduto.Checked;        &#13;&#10;    &#13;&#10;  Header_DetalheVenda.Visible  := ckDetalhaVenda.Checked;        &#13;&#10;  Child_DetalheDaVenda.Visible := ckDetalhaVenda.Checked;&#13;&#10;  Footer_DetalheVenda.Visible  := ckDetalhaVenda.Checked;&#13;&#10;    &#13;&#10;  Child_Cliente.Visible        := ckDetalhaCliente.Checked;&#13;&#10;  if Child_Cliente.Visible and not Child_DetalheDaVenda.Visible then&#13;&#10;    MasterData_Pedido.Child    := Child_Cliente;                              &#13;&#10;    &#13;&#10;  SumarioRelatorio.SQL.text := ' Select Sum(S.QUANT)as quantidade, SUM(S.TOTAL_IMPOSTOS)as TOTAL_IMPOSTOS, count(1), '+&#13;&#10;                               ' SUM(coalesce(S.VDESC,0))AS VDESC, ' +             &#13;&#10;                               ' SUM(coalesce(S.VOUTRO,0)) AS VOUTRO, ' + &#13;&#10;                               ' SUM(coalesce(S.NF_VFRETE,0)) AS FRETE, ' +&#13;&#10;                               ' SUM(coalesce(S.TOTAL_RELATORIO,0)) AS TOTAL_RELATORIO, ' +                                   &#13;&#10;                               'SUM(S.TOTAL)AS TOTAL, SUM(COALESCE(S.CUSTO,0)*COALESCE(S.QUANT,0))AS CUSTO, SUM(S.CALC_VLUCRO)AS LUCRO '+&#13;&#10;                               ' from ( '+                          &#13;&#10;                                Venda_Produto.SQL.text +&#13;&#10;                               ') S';&#13;&#10; // ShowMessage(SumarioRelatorio.SQL.text);                                                                                             &#13;&#10;&#13;&#10;  SumarioRelatorio.Close;&#13;&#10;  if ckDetalhaProduto.Checked then&#13;&#10;    DetailData_Produto.DataSet:= Venda                   &#13;&#10;  else&#13;&#10;    DetailData_Produto.DataSet:= nil;    &#13;&#10;  if ckDetalhaFinanceiro.Checked then&#13;&#10;    DetailData_Financiero.DataSet:= Venda_Financeiro&#13;&#10;  else&#13;&#10;    DetailData_Financiero.DataSet:= nil;      &#13;&#10;  if cktop10.checked then&#13;&#10;  begin              &#13;&#10;    QueryGrafico.SQL.text := ' Select (S.'+ACampoAgrupadorGrafico+')as LEGENDA, Sum(S.TOTAL)as Total '+&#13;&#10;                             ' from ( '+                          &#13;&#10;                             Venda_Produto.SQL.text +&#13;&#10;                             ') S group BY 1';&#13;&#10;    //ShowMessage(QueryGrafico.SQL.text);&#13;&#10;    QueryGrafico.Open;  &#13;&#10;  end;         &#13;&#10; // ShowMessage(Venda.SQL.text);&#13;&#10; // ShowMessage(SQL_Venda_Tipo); &#13;&#10;  Venda_Produto.Open;&#13;&#10;                      &#13;&#10;  if Venda_Produto.RecordCount = 0 then&#13;&#10;    Showmessage('Não há dados para serem exibidos!');&#13;&#10;  DialogPage1.ModalResult := mrOk;&#13;&#10;end;&#13;&#10;    &#13;&#10;procedure btncancelarOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  DialogPage1.ModalResult     := mrCancel;&#13;&#10;  Report.PreviewOptions.Modal := False;  &#13;&#10;end;&#13;&#10;&#13;&#10; /////////////SEM SQL A FRENTE///////////////////////////&#13;&#10;&#13;&#10;procedure Prepara_Grupo(ACampoAgrupador, ACaption :String);             &#13;&#10;begin     &#13;&#10;  ACampoAgrupadorGrafico                          := ACampoAgrupador;                        &#13;&#10;  GroupHeader_Pedido.Condition                    := 'Venda_Produto.&#34;'+ACampoAgrupador+'&#34;';&#13;&#10;  GrupoHeader_Pedido_OrderBy_Agrupador.Lines.Text := '[IIf( &#60;Venda_Produto.&#34;'+ACampoAgrupador+'&#34;&#62; = '''' , ''SEM '+ACaption+''' , &#60;Venda_Produto.&#34;'+ACampoAgrupador+'&#34;&#62; )]';&#13;&#10;  GrupoHeader_Pedido_OrderBy_Nome_Agrupado.Lines.Text := ACaption;&#13;&#10;  Memo_Total_grupo.Lines.Text := 'Total do Grupo: ' + ACaption;                                                                                                               &#13;&#10;  //GroupFooter_Pedido_OrderBy_Agrupador.Lines.Text := 'VENDAS - '+ACaption+': [SUM(&#60;Venda_Produto.&#34;TOTAL_RELATORIO&#34;&#62;,MasterData_Pedido,1)]';&#13;&#10;end;&#13;&#10;                &#13;&#10;procedure MasterData_PedidoOnMasterDetail(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if ckDetalhaFinanceiro.checked then&#13;&#10;  begin      &#13;&#10;    DetailData_Produto.Visible := NOt(Venda.FieldByName('DT_FATURAMENTO').AsString = '');&#13;&#10;    Header_Financeiro.Visible  := NOt(Venda.FieldByName('DT_FATURAMENTO').AsString = '');                                                &#13;&#10;  end;      &#13;&#10;end;    &#13;&#10; &#13;&#10;////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;  &#13;&#10;Function SQLWhere_Por_Empresa_E_Data:String;&#13;&#10;Var&#13;&#10;  Date1 : String;&#13;&#10;  Date2 : String;&#13;&#10;  ResultFiltros : String;&#13;&#10;  ResultFiltoData : String;                            &#13;&#10;begin                   &#13;&#10;  ///////////////////////////APLICA OS SQLs WHERE DE FILTROS\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\                                                                                                                                &#13;&#10;  // FILTRO EMPRESA&#13;&#10;  Result:= '';&#13;&#10;&#13;&#10;  ResultFiltoData := '';                                                          &#13;&#10;  ResultFiltros := '';&#13;&#10;    &#13;&#10;  if (VarToStr(&#60;CodEmpresa&#62;) &#60;&#62; '0')and(VarToStr(&#60;CodEmpresa&#62;) &#60;&#62; '') then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ '    (V.CODIGO like ''%-' + &#60;CodEmpresa&#62; + ''') ';  &#13;&#10;    EmpresaRel.SQL[3] := 'WHERE E.CODIGO = ''' + &#60;CodEmpresa&#62; + ''' ';&#13;&#10;    MemoFantasia.Lines.Text := &#60;EmpresaRel.&#34;FANTASIA&#34;&#62;;&#13;&#10;    MemoEndereco.Lines.Text := &#60;EmpresaRel.&#34;ENDERECO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;BAIRRO&#34;&#62; + ' - ' + &#60;EmpresaRel.&#34;CIDADE&#34;&#62; + '/' + &#60;EmpresaRel.&#34;UF&#34;&#62; + #13#10 +      &#13;&#10;                             'CEP: ' + &#60;EmpresaRel.&#34;CEP&#34;&#62; +  ' CNPJ: ' + &#60;EmpresaRel.&#34;CNPJCPF&#34;&#62; + ' INSC: ' + &#60;EmpresaRel.&#34;IEST&#34;&#62; + #13#10 +&#13;&#10;                             'Fone: ' + &#60;EmpresaRel.&#34;FONE1&#34;&#62; + ' / Fax: ' + &#60;EmpresaRel.&#34;FAX&#34;&#62;;              &#13;&#10;  end else&#13;&#10;    ResultFiltros := ResultFiltros + 'Empresa selecionada: [EmpresaRel.&#34;FANTASIA&#34;] | ';&#13;&#10;//  ShowMessage(VarToStr(&#60;TipoFiltroData&#62;));                        &#13;&#10;  try                                                                      &#13;&#10;    // FILTRO DATAS                                                                                                                                &#13;&#10;    Date1 := FormatDateTime('DD.MM.YYYY', &#60;DtInicio&#62;);&#13;&#10;    Date2 := FormatDateTime('DD.MM.YYYY 23:59:59', &#60;DtFim&#62;);           &#13;&#10;    case &#60;TipoFiltroData&#62; of&#13;&#10;      0 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_EMISSAO between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) ';&#13;&#10;        ResultFiltoData := ResultFiltoData + 'Data de Emissão: [DtInicio] à [DtFim]';                                                                                &#13;&#10;      end;            &#13;&#10;      1 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_FATURAMENTO between ''' + Date1 + ''' and coalesce(''' + Date2 + ''', current_timestamp)) ';&#13;&#10;        ResultFiltoData := ResultFiltoData + 'Data de Faturamento: [DtInicio] à [DtFim]';  &#13;&#10;      end;            &#13;&#10;      2 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and  (V.DT_COMPETENCIA between ''' + FormatDateTime('DD.MM.YYYY', &#60;DtInicio&#62;) + ''' and coalesce(''' + FormatDateTime('DD.MM.YYYY', &#60;DtFim&#62;) + ''', current_timestamp)) ';              &#13;&#10;        ResultFiltoData := ResultFiltoData + 'Data de Competência: [DtInicio] à [DtFim]';  &#13;&#10;      end;                      &#13;&#10;    end;&#13;&#10;  except end;&#13;&#10;//  ShowMessage(Result);        &#13;&#10;  try&#13;&#10;&#13;&#10;  // FILTRO DE MODELO DE TIPO DE VENDA&#13;&#10;  if VarToStr(&#60;ModeloTipoVenda&#62;) = 'VENDA' then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.IS_VENDA = ''S'') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Modelo: Venda';        &#13;&#10;  end else&#13;&#10;  if VarToStr(&#60;ModeloTipoVenda&#62;) = 'ORCAMENTO' then  &#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.IS_ORCAMENTO = ''S'') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Modelo: Orçamento';&#13;&#10;  end else&#13;&#10;  if VarToStr(&#60;ModeloTipoVenda&#62;) = 'NOTA FISCAL' then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.IS_NOTAFISCAL = ''S'') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Modelo: Nota Fiscal';  &#13;&#10;  end;&#13;&#10;&#13;&#10;  // FILTRO DE TIPO DE VENDA&#13;&#10;  if (VarToStr(&#60;TipoVenda&#62;) &#60;&#62; 'TODOS')and(VarToStr(&#60;TipoVenda&#62;)&#60;&#62;'') then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.VENDA_TIPO = ''' + &#60;TipoVenda&#62; + ''') ';  &#13;&#10;    ResultFiltros := ResultFiltros + '  |  Tipo de Venda: [TipoVenda]';              &#13;&#10;  end;        &#13;&#10;    &#13;&#10;    //  FILTRO SELECAO (FATURADOS/NÃO FATURADOS/EXCLUIDOS)                                                                                                                                   &#13;&#10;    case &#60;TipoSelecao&#62; of              &#13;&#10;      0 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.dt_faturamento is not null) and (v.status like ''ATIVO%'') ';&#13;&#10;        ResultFiltros := ResultFiltros + '  |  Selecionado: Faturadas';&#13;&#10;      end;&#13;&#10;      1 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.dt_faturamento is null) and (v.status like ''ATIVO%'') ';&#13;&#10;        ResultFiltros := ResultFiltros + '  |  Selecionado: Não Faturadas';&#13;&#10;//      Venda_financeiro.SQL[4] := ' and (:dt_faturamento is not null)';   &#13;&#10;      end;          &#13;&#10;      2 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ 'and(v.status like ''INATIVO%'') ';&#13;&#10;        ResultFiltros := ResultFiltros + '  |  Selecionado: Canceladas/Excluídas';&#13;&#10; //     Venda_financeiro.SQL[4] := 'and(:status like ''INATIVO%'') ';                      &#13;&#10;      end;&#13;&#10;      3 : &#13;&#10;      begin&#13;&#10;        Result:= Result+ ' and (v.status like ''ATIVO%'') ';&#13;&#10;        ResultFiltros := ResultFiltros + '  |  Selecionado: Faturadas e Não Faturadas';                      &#13;&#10;      end;                      &#13;&#10;    end;&#13;&#10;  except end;&#13;&#10;&#13;&#10;//  ShowMessage(Result);    &#13;&#10;   // FILTRO CLIENTE&#13;&#10;  if VarToStr(&#60;CodCliente&#62;) &#60;&#62; '' then&#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.PESSOA_RESPONSAVEL_CODIGO = ''' + &#60;CodCliente&#62; + ''') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Cliente: [Venda_Produto.&#34;RAZAOSOCIAL&#34;]';        &#13;&#10;  end;&#13;&#10;//  ShowMessage(Result);  &#13;&#10;  // FILTRO FUNCIONÁRIO&#13;&#10;  if VarToStr(&#60;CodFuncionario&#62;) &#60;&#62; '' then        &#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.PESSOA_FUNCIONARIO_CODIGO = ''' + &#60;CodFuncionario&#62; + ''') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Funcionário: [Venda_Produto.&#34;P_FUNCIONARIO&#34;]';            &#13;&#10;  end;&#13;&#10;//  ShowMessage(Result);  &#13;&#10;  // FILTRO REPRESENTANTE&#13;&#10;  if VarToStr(&#60;CodRepresentante&#62;) &#60;&#62; '' then        &#13;&#10;  begin              &#13;&#10;    Result:= Result+ ' and (V.PESSOA_REPRESENTANTE_CODIGO = ''' + &#60;CodRepresentante&#62; + ''') ';&#13;&#10;    ResultFiltros := ResultFiltros + '  |  Representante: [V