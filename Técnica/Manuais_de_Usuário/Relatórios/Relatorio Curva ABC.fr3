<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="45014,709363287" ReportOptions.Description.Text="" ReportOptions.LastChange="45014,709363287" ScriptLanguage="PascalScript" ScriptText.Text="// RELATÓRIO DE CURVA ABC - BY JOÃO B. - VERSÃO 2023.1.0 - 03/04/2023                                                                                                                                                                                                 &#13;&#10;  &#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;Date1 : String;&#13;&#10;Date2 : String;&#13;&#10;ASQL : String;&#13;&#10;ResultFiltros : String;&#13;&#10;ResultFiltroDT : String;              &#13;&#10;begin&#13;&#10;&#13;&#10;// PRIMEIROS PARAMETROS DAS VARIAVEIS                                                                                                       &#13;&#10;  Date1 := FormatDateTime('DD.MM.YYYY 00:00:00', &#60;DtInicio&#62;);&#13;&#10;  Date2 := FormatDateTime('DD.MM.YYYY 23:59:59', &#60;DtFim&#62;);      &#13;&#10;  ResultFiltros := '';&#13;&#10;  ResultFiltroDT := '';                                                                  &#13;&#10;    &#13;&#10;//// SELECT DA QUERY ////////////////////////////////////////////////////////////////////////////////////////////////////////                                                  &#13;&#10;    &#13;&#10;  ASQL := 'select R4.CODPRODUTO, R4.DESCRICAO, R4.QUANT, R4.TOTAL_CUSTO, R4.TOTAL_ITEM, R4.CUSTO_MEDIO, R4.TICKET_MEDIO, ' +          &#13;&#10;      ' R4.LUCRO_BRUTO, R4.TOTAL_GERAL, R4.PERC, R4.PERC_ACUMULADO, ' +         &#13;&#10;      ' case ' +       &#13;&#10;         ' when R4.PERC_ACUMULADO &#60;= 80 then ''A'' ' +  &#13;&#10;         ' when R4.PERC_ACUMULADO &#60;= 95 then ''B'' ' +  &#13;&#10;         ' else ''C'' ' +  &#13;&#10;       ' end CLASSE ' +  &#13;&#10;' from (select R3.CODPRODUTO, R3.DESCRICAO, R3.QUANT, R3.TOTAL_CUSTO, R3.TOTAL_ITEM, R3.CUSTO_MEDIO, R3.TICKET_MEDIO, ' +  &#13;&#10;             ' R3.LUCRO_BRUTO, R3.TOTAL_GERAL, R3.PERC, sum(R3.PERC) over(order by R3.PERC desc) PERC_ACUMULADO ' +  &#13;&#10;      ' from (select R2.CODPRODUTO, R2.DESCRICAO, R2.QUANT, R2.TOTAL_CUSTO, R2.TOTAL_ITEM, R2.CUSTO_MEDIO, ' +  &#13;&#10;                   ' R2.TICKET_MEDIO, R2.LUCRO_BRUTO, sum(R2.TOTAL_ITEM) over() TOTAL_GERAL, ' +  &#13;&#10;                   ' cast(R2.TOTAL_ITEM as numeric(15,3)) / cast(sum(R2.TOTAL_ITEM) over() as numeric(15,3)) * 100 PERC ' +  &#13;&#10;            ' from (select R.CODPRODUTO, R.DESCRICAO, R.QUANT, R.TOTAL_CUSTO, R.TOTAL_ITEM, ' +  &#13;&#10;                         ' sum(R.TOTAL_CUSTO / coalesce(R.QUANT, 0)) CUSTO_MEDIO, ' +  &#13;&#10;                         ' sum(R.TOTAL_ITEM / coalesce(R.QUANT, 0)) TICKET_MEDIO, sum(R.TOTAL_ITEM - R.TOTAL_CUSTO) LUCRO_BRUTO ' +  &#13;&#10;&#13;&#10;                  ' from (select VP.CODPRODUTO, VP.DESCRICAO, ' +  &#13;&#10;                               ' sum(coalesce(VP.QUANT, 0) * coalesce(VP.CUSTO, 0)) TOTAL_CUSTO, sum(VP.QUANT) QUANT, ' +  &#13;&#10;                               ' sum(VP.TOTAL_RELATORIO) TOTAL_ITEM ' +  &#13;&#10;                        ' from VENDA_PRODUTO VP ' +  &#13;&#10;                        ' left join VENDA V on (VP.CODVENDA = V.CODIGO) ';  &#13;&#10;                                &#13;&#10;//// FIM DO SELECT DA QUERY //////////////////////////////////////////////////////////////////////////////////////////////////////                                                                                                                                                                                                                                                                                   &#13;&#10;      &#13;&#10;//// WHERE DA QUERY /////////////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;&#13;&#10;// FILTRO SELECAO                                           &#13;&#10;  case &#60;TipoSelecao&#62; of    &#13;&#10;    0:&#13;&#10;    begin&#13;&#10;      ASQL := ASQL + ' where V.DT_FATURAMENTO is not null and ' +  &#13;&#10;                     ' V.ATIVO = ''S'' and ' +                         &#13;&#10;                     ' V.STATUS like ''ATIVO%'' ';&#13;&#10;      ResultFiltros := ResultFiltros + ' Selecionado: Vendas Faturadas';                                                                                                                                                                                                                          &#13;&#10;    end;&#13;&#10;    1:&#13;&#10;    begin&#13;&#10;      ASQL := ASQL + ' where V.DT_FATURAMENTO is null and ' +  &#13;&#10;                     ' V.ATIVO = ''S'' and ' +                         &#13;&#10;                     ' V.STATUS like ''ATIVO%'' ';&#13;&#10;      ResultFiltros := ResultFiltros + ' Selecionado: Vendas não Faturadas';                                             &#13;&#10;    end;&#13;&#10;    2:&#13;&#10;    begin&#13;&#10;      ASQL := ASQL + ' where V.ATIVO = ''N'' and ' +                         &#13;&#10;                     ' V.STATUS like ''INATIVO%'' ';&#13;&#10;      ResultFiltros := ResultFiltros + ' Selecionado: Vendas Canceladas/Excluídas';                                             &#13;&#10;    end;&#13;&#10;    3:&#13;&#10;    begin&#13;&#10;      ASQL := ASQL + ' where V.ATIVO = ''S'' and ' +&#13;&#10;                     ' V.STATUS like ''ATIVO%'' ';&#13;&#10;      ResultFiltros := ResultFiltros + ' Selecionado: Todas as Vendas Ativas';                                             &#13;&#10;    end;&#13;&#10;  end;                            &#13;&#10;&#13;&#10;// FILTRO PELO MODELO DE VENDA                                                                                     &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'VENDA' then&#13;&#10;  begin                        &#13;&#10;    ASQL := ASQL + ' and V.IS_VENDA = ''S'' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Modelo: Venda';                                                       &#13;&#10;  end;            &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'ORCAMENTO' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and V.IS_ORCAMENTO = ''S'' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Modelo: Orçamento';                                                       &#13;&#10;  end;            &#13;&#10;  if &#60;ModeloTipoVenda&#62; = 'NOTA FISCAL' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and V.IS_NOTAFISCAL = ''S'' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Modelo: Nota Fiscal';                                                       &#13;&#10;  end;            &#13;&#10;&#13;&#10;// FILTRO TIPO DE VENDA&#13;&#10;  if &#60;TipoVenda&#62; &#60;&#62; 'TODOS' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and V.VENDA_TIPO = ''' + &#60;TipoVenda&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Tipo de Venda: '+ &#60;TipoVenda&#62;;                                                       &#13;&#10;  end;            &#13;&#10;&#13;&#10;// FILTRO POR PRODUTO&#13;&#10;  if &#60;CodProduto&#62; &#60;&#62; '' then ShowMessage('Filtro por produto não pode ser aplicado nesse relatório. Será exibido o relatório sem este filtro.');                                                           &#13;&#10;&#13;&#10;// FILTRO POR GRUPO DE PRODUTO&#13;&#10;  if &#60;CodProdutoGrupo&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and VP.CODPRODUTO_GRUPO = ''' + &#60;CodProdutoGrupo&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Grupo de Produto: '+ &#60;ProdutoGrupo&#62;;                                                       &#13;&#10;  end;          &#13;&#10;// FILTRO POR FUNCIONARIO                                                                                &#13;&#10;  if &#60;CodFuncionario&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and V.PESSOA_FUNCIONARIO_CODIGO = ''' + &#60;CodFuncionario&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Funcionário: '+ &#60;Funcionario&#62;;                                                       &#13;&#10;  end;              &#13;&#10;&#13;&#10;// FILTRO POR REPRESENTANTE&#13;&#10;  if &#60;CodRepresentante&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and V.PESSOA_REPRESENTANTE_CODIGO = ''' + &#60;CodRepresentante&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Representante: '+ &#60;Representante&#62;;                                                       &#13;&#10;  end;              &#13;&#10;&#13;&#10;// FILTRO POR MARCA&#13;&#10;  if &#60;CodMarca&#62; &#60;&#62; '' then&#13;&#10;  begin&#13;&#10;    ASQL := ASQL + ' and VP.CODPRODUTO_MARCA = ''' + &#60;CodMarca&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Funcionário: '+ &#60;Marca&#62;;                                                       &#13;&#10;  end;                &#13;&#10;&#13;&#10;// FILTRO POR FORNECEDOR&#13;&#10;  if &#60;CodFornecedor&#62; &#60;&#62; '' then&#13;&#10;  begin      &#13;&#10;    ASQL := ASQL + ' and VP.PESSOA_FORNECEDOR_CODIGO = ''' + &#60;CodFornecedor&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Fornecedor: '+ &#60;Fornecedor&#62;;                                                       &#13;&#10;  end;              &#13;&#10;&#13;&#10;// FILTRO POR CLIENTE&#13;&#10;  if &#60;CodCliente&#62; &#60;&#62; '' then&#13;&#10;  begin      &#13;&#10;    ASQL := ASQL + ' and V.PESSOA_RESPONSAVEL_CODIGO = ''' + &#60;CodCliente&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Cliente: '+ &#60;Cliente&#62;;                                                       &#13;&#10;  end;              &#13;&#10;&#13;&#10;// FILTRO POR GRUPO DE CLIENTE&#13;&#10;//  if &#60;CodClienteGrupo&#62; &#60;&#62; '' then ASQL := ASQL + ' and V. ///// Precisa de Left Join&#13;&#10;&#13;&#10;// FILTRO POR USUÁRIO&#13;&#10;  if &#60;CodUsuario&#62; &#60;&#62; '' then&#13;&#10;  begin                        &#13;&#10;    ASQL := ASQL + ' and V.CODUSUARIO = ''' + &#60;CodUsuario&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Usuário: '+ &#60;Usuario&#62;;                                                       &#13;&#10;  end;                &#13;&#10;&#13;&#10;//FILTRO POR CIDADE&#13;&#10;  if &#60;CodCidade&#62; &#60;&#62; '' then&#13;&#10;  begin      &#13;&#10;    ASQL := ASQL + ' and V.CODCIDADE = ''' + &#60;CodCidade&#62; + ''' ';&#13;&#10;    ResultFiltros := ResultFiltros + ' | Cidade: ';                                                       &#13;&#10;  end;              &#13;&#10;  &#13;&#10;// FILTRO DE DATA&#13;&#10;  case &#60;TipoFiltroData&#62; of&#13;&#10;    0:&#13;&#10;    begin                    &#13;&#10;    ASQL := ASQL + 'and V.DT_EMISSAO between ''' + Date1 + ''' and '''+ Date2 + ''''+                &#13;&#10;                        ' group by 1, 2 ' +       &#13;&#10;                        ' order by 5 desc) R ' +          &#13;&#10;                  ' group by 1, 2, 3, 4, 5) R2) R3) R4 ';                                            &#13;&#10;    ResultFiltroDT := ' Data de Emissão: '+ FormatDateTime('DD/MM/YYYY', &#60;DtInicio&#62;) + ' à '+ FormatDateTime('DD/MM/YYYY', &#60;DtFim&#62;);                                &#13;&#10;    end;&#13;&#10;    1:&#13;&#10;    begin&#13;&#10;    ASQL := ASQL + 'and V.DT_FATURAMENTO between ''' + Date1 + ''' and '''+ Date2 + ''''+                &#13;&#10;                        ' group by 1, 2 ' +       &#13;&#10;                        ' order by 5 desc) R ' +          &#13;&#10;                  ' group by 1, 2, 3, 4, 5) R2) R3) R4 ';&#13;&#10;    ResultFiltroDT := ' Data de Faturamento: '+ FormatDateTime('DD/MM/YYYY', &#60;DtInicio&#62;) + ' à '+ FormatDateTime('DD/MM/YYYY', &#60;DtFim&#62;);                                  &#13;&#10;    end;&#13;&#10;    2:&#13;&#10;    begin&#13;&#10;    ASQL := ASQL + 'and V.DT_COMPETENCIA between ''' + Date1 + ''' and '''+ Date2 + ''''+                &#13;&#10;                        ' group by 1, 2 ' +       &#13;&#10;                        ' order by 5 desc) R ' +          &#13;&#10;                  ' group by 1, 2, 3, 4, 5) R2) R3) R4 ';&#13;&#10;    ResultFiltroDT := ' Data de Competência: '+ FormatDateTime('DD/MM/YYYY', &#60;DtInicio&#62;) + ' à '+ FormatDateTime('DD/MM/YYYY', &#60;DtFim&#62;);                                  &#13;&#10;    end;&#13;&#10;  end;                  &#13;&#10;                      &#13;&#10;//// FIM DO WHERE DA QUERY ////////////////////////////////////////////////////////////////////////////////////////////////////&#13;&#10;        &#13;&#10;&#13;&#10;//// ADD O SQL A QUERY ///////////////////////////////////////////////////////////////////////////////////////////////////////      &#13;&#10;  querProduto.SQL.Add(ASQL);&#13;&#10;&#13;&#10;// MEMO RECEBE A INFORMAÇÃO DOS FILTROS ATIVOS                                                                                                                                                &#13;&#10;  mFiltros.Text := ResultFiltros;&#13;&#10;  Memo17.Text := ResultFiltroDT;&#13;&#10;// ShowMessage(querProduto.SQL.Text);                                &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure Memo2OnAfterData(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;// DEFINE A COR DA CLASSE                                                                                           &#13;&#10;  if Memo2.Value = 'A' then memoFundinho.color := $00BDFBCA;&#13;&#10;  if Memo2.Value = 'B' then memoFundinho.color := $00F4E0BD;&#13;&#10;  if Memo2.Value = 'C' then memoFundinho.color := $00C7CEFA;                                                                                                                               &#13;&#10;end;&#13;&#10;&#13;&#10;procedure querProdutoTOTAL_ITEMOnAfterData(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  querProdutoTOTAL_ITEM.Text := FormatFloat('###,###,###,##0.00#', &#60;querProduto.&#34;TOTAL_ITEM&#34;&#62;);  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  CarregaLogoEmpresa(Logo);                       &#13;&#10;end.                       " StoreInDFM="False" OnReportPrint="frxReportOnReportPrint">
