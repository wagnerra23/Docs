<TfrxReport Tag="1" Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42117,4652342245" ReportOptions.Description.Text="[Dados]&#13;&#10;Versao=2022.0.0.3&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2022.0.0.2&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=2022.0.0.1&#13;&#10;Link=&#13;&#10;Observacao=[Dados]&#13;&#10;Versao=0.0.0.0&#13;&#10;Link=&#13;&#10;Observacao=&#13;&#10;Form=FrmPontoEletronico&#13;&#10;&#13;&#10;Form=TFrmFramePessoasFichaPonto&#13;&#10;&#13;&#10;Form=TFrmFramePessoasFichaPonto&#13;&#10;&#13;&#10;Form=TFrmFramePessoasFichaPonto" ReportOptions.LastChange="42117,4652342245" ReportOptions.VersionBuild="3" ReportOptions.VersionMajor="2022" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="0" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  MinutosSegunda, MinutosTerca, MinutosQuarta, MinutosQuinta, &#13;&#10;  MinutosSexta, MinutosSabado, MinutosDomingo, MinutosExtras, &#13;&#10;  MinutosFaltantes, MinutosTotaisExtras, MinutosTotaisFaltantes, &#13;&#10;  QntDivergenteExtra, QntDivergenteFaltantes : Integer;&#13;&#10;  Extras, Faltantes, TotalExtras, TotalFaltantes, SomaExtras, SomaFaltantes : string;                                                                       &#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  AVazio : Boolean;&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;Horarios.&#34;MINUTOS&#34;&#62;) = '0' then&#13;&#10;     exit;&#13;&#10;  AVazio := (&#60;Horarios.&#34;FINAL1&#34;&#62; = '00:00')&#13;&#10;             and (&#60;Horarios.&#34;FINAL2&#34;&#62;  = '00:00')      &#13;&#10;             and (&#60;Horarios.&#34;INICIO1&#34;&#62; = '00:00')        &#13;&#10;             and (&#60;Horarios.&#34;INICIO2&#34;&#62; = '00:00');                                                   &#13;&#10;  Memo15.Lines.Text := iif(&#60;Horarios.&#34;INICIO1&#34;&#62; = '00:00', '', FormatDateTime('HH:MM', &#60;Horarios.&#34;INICIO1&#34;&#62;));&#13;&#10;  Memo16.Lines.Text := iif(&#60;Horarios.&#34;FINAL1&#34;&#62;  = '00:00', '', FormatDateTime('HH:MM', &#60;Horarios.&#34;FINAL1&#34;&#62;));&#13;&#10;  Memo17.Lines.Text := iif(&#60;Horarios.&#34;INICIO2&#34;&#62; = '00:00', '', FormatDateTime('HH:MM', &#60;Horarios.&#34;INICIO2&#34;&#62;));&#13;&#10;  Memo18.Lines.Text := iif(&#60;Horarios.&#34;FINAL2&#34;&#62;  = '00:00', '', FormatDateTime('HH:MM', &#60;Horarios.&#34;FINAL2&#34;&#62;));&#13;&#10;  Memo14.Lines.Text := iif(AVazio, '', &#60;Horarios.&#34;CODIGO_HORARIO&#34;&#62;);&#13;&#10;  Memo39.Lines.Text := iif(&#60;Horarios.&#34;MINUTOS&#34;&#62; = 0, '', FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(&#60;Horarios.&#34;MINUTOS&#34;&#62;))));&#13;&#10;&#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 1 then MinutosSegunda := &#60;Horarios.&#34;MINUTOS&#34;&#62;;&#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 2 then MinutosTerca   := &#60;Horarios.&#34;MINUTOS&#34;&#62;;&#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 3 then MinutosQuarta  := &#60;Horarios.&#34;MINUTOS&#34;&#62;;&#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 4 then MinutosQuinta  := &#60;Horarios.&#34;MINUTOS&#34;&#62;;&#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 5 then MinutosSexta   := &#60;Horarios.&#34;MINUTOS&#34;&#62;;  &#13;&#10;  if Horarios.FieldByName('SEQ').AsInteger = 6 then MinutosSabado  := &#60;Horarios.&#34;MINUTOS&#34;&#62;;   &#13;&#10;//  if Horarios.FieldByName('SEQ').AsInteger = 7 then MinutosDomingo := &#60;Horarios.&#34;MINUTOS&#34;&#62;;&#13;&#10;//  ShowMessage('Aqui Masterdata Fim = '+ Horarios.FieldByName('SEQ').AsSTring);                                          &#13;&#10;                                            &#13;&#10;end;        &#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin                                          &#13;&#10;  MinutosTotaisFaltantes := 0;&#13;&#10;  MinutosTotaisExtras    := 0;                    &#13;&#10;  if MonthOf(&#60;dtinicio&#62;) in [2, 4, 6, 9, 11] then&#13;&#10;    MasterData2.RowCount := 30;&#13;&#10;  Memo43.Text := 'Período: ' + FormatDateTime('DD/MM/YYYY', &#60;dtinicio&#62;) + ' à ' + FormatDateTime('DD/MM/YYYY', &#60;dtfim&#62;);      &#13;&#10;//  ShowMessage('Aqui Page1OnBeforePrint');                                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Horarios.Close;                                       &#13;&#10;  Horarios.SQL.Text := 'select * ' +       &#13;&#10;                       'from (select 1 as SEQ,  FH.CODFUNCIONARIO, ''Segunda-Feira'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_SEGUNDA_INICIAL as Inicio1, FH.MAT_SEGUNDA_FINAL as Final1, ' +  &#13;&#10;                       '            FH.VES_SEGUNDA_INICIAL as Inicio2, FH.VES_SEGUNDA_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_SEGUNDA_INICIAL to FH.MAT_SEGUNDA_FINAL) + datediff(minute from FH.VES_SEGUNDA_INICIAL to FH.VES_SEGUNDA_FINAL)) as MINUTOS ' +                               &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH ' +  &#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 2 as SEQ, FH.CODFUNCIONARIO, ''Terça-Feira'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_TERCA_INICIAL as Inicio1, FH.MAT_TERCA_FINAL as Final1, FH.VES_TERCA_INICIAL as Inicio2, ' +  &#13;&#10;                       '            FH.VES_TERCA_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_TERCA_INICIAL to FH.MAT_TERCA_FINAL) + datediff(minute from FH.VES_TERCA_INICIAL to FH.VES_TERCA_FINAL)) as MINUTOS ' +                              &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH ' +  &#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 3 as SEQ, FH.CODFUNCIONARIO, ''Quarta-Feira'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_QUARTA_INICIAL as Inicio1, FH.MAT_QUARTA_FINAL as Final1, FH.VES_QUARTA_INICIAL as Inicio2, ' +  &#13;&#10;                       '            FH.VES_QUARTA_FINAL as Final, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_QUARTA_INICIAL to FH.MAT_QUARTA_FINAL) + datediff(minute from FH.VES_QUARTA_INICIAL to FH.VES_QUARTA_FINAL)) as MINUTOS ' +                           &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH ' +  &#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 4 as SEQ, FH.CODFUNCIONARIO, ''Quinta-Feira'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_QUINTA_INICIAL as Inicio1, FH.MAT_QUINTA_FINAL as Final1, FH.VES_QUINTA_INICIAL as Inicio2, ' +  &#13;&#10;                       '            FH.VES_QUINTA_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_QUINTA_INICIAL to FH.MAT_QUINTA_FINAL) + datediff(minute from FH.VES_QUINTA_INICIAL to FH.VES_QUINTA_FINAL)) as MINUTOS ' +                           &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH ' +  &#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 5 as SEQ, FH.CODFUNCIONARIO, ''Sexta-Feira'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_SEXTA_INICIAL as Inicio1, FH.MAT_SEXTA_FINAL as Final1, FH.VES_SEXTA_INICIAL as Inicio2, ' +  &#13;&#10;                       '            FH.VES_SEXTA_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_SEXTA_INICIAL to FH.MAT_SEXTA_FINAL) + datediff(minute from FH.VES_SEXTA_INICIAL to FH.VES_SEXTA_FINAL)) as MINUTOS ' +                           &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH ' +&#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 6 as SEQ, FH.CODFUNCIONARIO, ''Sábado'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_SABADO_INICIAL as Inicio1, FH.MAT_SABADO_FINAL as Final1, FH.VES_SABADO_INICIAL as Inicio2, ' +  &#13;&#10;                       '            FH.VES_SABADO_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_SABADO_INICIAL to FH.MAT_SABADO_FINAL) + datediff(minute from FH.VES_SABADO_INICIAL to FH.VES_SABADO_FINAL)) as MINUTOS ' +                                                                                                                                             &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH) '&#13;&#10;                       { +&#13;&#10;                       '     union ' +  &#13;&#10;                       '     select 7 as SEQ, FH.CODFUNCIONARIO, ''Domingo'' as CODIGO_HORARIO, ' +  &#13;&#10;                       '            FH.MAT_DOMINGO_INICIAL as Inicio1, FH.MAT_DOMINGO_FINAL as Final1, ' +  &#13;&#10;                       '            FH.VES_DOMINGO_INICIAL as Inicio2, FH.VES_DOMINGO_FINAL as Final2, ' +&#13;&#10;                       '            (datediff(minute from FH.MAT_DOMINGO_INICIAL to FH.MAT_DOMINGO_FINAL) + datediff(minute from FH.VES_DOMINGO_INICIAL to FH.VES_DOMINGO_FINAL)) as MINUTOS ' +                                                                                                                                             &#13;&#10;                       '     from FUNCIONARIO_HORARIO FH) '};              &#13;&#10;  Horarios.SQL.Add( 'where CODFUNCIONARIO = :CODFUNCIONARIO');                                &#13;&#10;  Horarios.ParamByName('CODFUNCIONARIO').Value := &#60;CodFuncionario&#62;;&#13;&#10;//  ShowMessage(Horarios.SQL.Text);                                            &#13;&#10;  Horarios.Open;&#13;&#10;&#13;&#10;//----------------------------------------------------------                                                                                                                        &#13;&#10;&#13;&#10;  Lista_horarios.Close;&#13;&#10;  Lista_horarios.SQL.Text := 'select cast(FA.DATA_HORARIO_MARCACAO as date) as DATA_HORARIO_MARCACAO, FA.CODPESSOA_FUNCIONARIO, extract(day from FA.DATA_HORARIO_MARCACAO) as DIA, ' +          &#13;&#10;                             '       list(lpad(extract(hour from FA.DATA_HORARIO_MARCACAO), 2, ''0'') || '':'' || lpad(extract(minute from FA.DATA_HORARIO_MARCACAO), 2, ''0''), ''  '') as LISTA ' +          &#13;&#10;                             'from FUNCIONARIO_PONTO_ARQUIVO FA ' +&#13;&#10;                             'where FA.CODPESSOA_FUNCIONARIO = :CODFUNCIONARIO ' +       &#13;&#10;                             '       and FA.DATA_HORARIO_MARCACAO between :DTINICIO and :DTFIM ' +       &#13;&#10;                             'group by FA.CODPESSOA_FUNCIONARIO, extract(day from FA.DATA_HORARIO_MARCACAO), cast(FA.DATA_HORARIO_MARCACAO as date)'+&#13;&#10;                             ' order by 1';                                                                  &#13;&#10;  Lista_horarios.ParamByName('CODFUNCIONARIO').Value := &#60;CodFuncionario&#62;;&#13;&#10;  Lista_horarios.ParamByName('DTINICIO').Value       := &#60;dtinicio&#62;;             &#13;&#10;  Lista_horarios.ParamByName('DTFIM').Value          := &#60;dtfim&#62;;&#13;&#10;  //ShowMessage(Lista_horarios.SQL.Text);      &#13;&#10;  Lista_horarios.Open;                                          &#13;&#10;  &#13;&#10;//----------------------------------------------------------&#13;&#10;  Jornada.Close;&#13;&#10;  Jornada.SQL.Text := 'select extract(day from FP.DIA_REFERENCIA) as DIA, FP.ENTRADA1, FP.SAIDA1, ' +          &#13;&#10;                      '      cast(FP.DIA_REFERENCIA as date) as DIA_REFERENCIA , FP.ENTRADA2, FP.SAIDA2, ' +&#13;&#10;                      '      coalesce(datediff(minute from FP.ENTRADA1 to FP.SAIDA1), 0) + ' +       &#13;&#10;                      '      coalesce(datediff(minute from FP.ENTRADA2 to FP.SAIDA2), 0) as MINUTOS_TRABALHADOS, ' +&#13;&#10;                      '      FP.FALTA_JUSTIFICADA, FP.OBSERVACAO ' +                                                    &#13;&#10;                      'from FUNCIONARIO_PONTO FP ' +          &#13;&#10;                      'where FP.CODFUNCIONARIO = :CODFUNCIONARIO ' +          &#13;&#10;                      '      and FP.DIA_REFERENCIA between ''' + FormatDateTime('MM/DD/YYYY', &#60;dtinicio&#62;) + ''' and ''' + FormatDateTime('MM/DD/YYYY 23:59:59', &#60;dtfim&#62;) + '''' +  // :DTFIM ';&#13;&#10;                      ' order by FP.DIA_REFERENCIA ';                                        &#13;&#10;  Jornada.ParamByName('CODFUNCIONARIO').Value  := &#60;CodFuncionario&#62;;&#13;&#10;  //Jornada.ParamByName('DTINICIO').Value        := &#60;dtinicio&#62;;&#13;&#10;  &#13;&#10;  //ADataStr := DateTimeToStr(AData) + '23:59:59' ;                                  &#13;&#10;  //Jornada.ParamByName('DTFIM').Value           := WR2EndOfTheDay(AData);&#13;&#10;  //ShowMessage(Jornada.SQL.Text  + #13#10#13#10 + &#13;&#10;  //            VarToStr(&#60;CodFuncionario&#62;) + #13#10#13#10 + &#13;&#10;  //            VarToStr(&#60;dtinicio&#62;) + #13#10#13#1 + VarToStr(AData));&#13;&#10;                                          &#13;&#10;  Jornada.Open;&#13;&#10;  Jornada.First;            &#13;&#10;//  ShowMessage('Aqui Header1OnBeforePrint');                                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  ACodDia, Minutos : Integer;&#13;&#10;begin&#13;&#10;//  ShowMessage('Aqui inicio  MasterData2OnBeforePrint');           &#13;&#10;                                          &#13;&#10;  if Lista_horarios.Locate('DATA_HORARIO_MARCACAO', Jornada.FieldByName('DIA_REFERENCIA').Value , nil) then&#13;&#10;  begin&#13;&#10;    MemListaHorarios.Text := Lista_horarios.FieldByName('LISTA').AsString;  &#13;&#10;   // Memo49.Text           := ' ' + Lista_horarios.FieldByName('LISTA').AsString              &#13;&#10;  end else&#13;&#10;  begin&#13;&#10;    MemListaHorarios.Text := '';&#13;&#10;   // Memo49.Text           := '';                                                                                                             &#13;&#10;  end;       &#13;&#10;//  Line.Text:=                    &#13;&#10;  &#13;&#10; { ShowMessage(IntToStr(MinutosSegunda));&#13;&#10;  ShowMessage(IntToStr(MinutosTerca));&#13;&#10;  ShowMessage(IntToStr(MinutosQuarta));&#13;&#10;  ShowMessage(IntToStr(MinutosQuinta));&#13;&#10;  ShowMessage(IntToStr(MinutosSexta));&#13;&#10;  ShowMessage(IntToStr(MinutosSabado));&#13;&#10;  ShowMessage(IntToStr(MinutosDomingo));     }            &#13;&#10;    &#13;&#10; // if Jornada.Locate('DIA', &#60;Line#&#62; , nil) then&#13;&#10; // begin             &#13;&#10;    Memo33.Text := iif(FormatDateTime('HH:MM', Jornada.FieldByName('ENTRADA1').AsDateTime) = '00:00', '', FormatDateTime('HH:MM' ,Jornada.FieldByName('ENTRADA1').AsDateTime));    &#13;&#10;    Memo34.Text := iif(FormatDateTime('HH:MM', Jornada.FieldByName('SAIDA1').AsDateTime) = '00:00', '', FormatDateTime('HH:MM', Jornada.FieldByName('SAIDA1').AsDateTime));    &#13;&#10;    Memo35.Text := iif(FormatDateTime('HH:MM', Jornada.FieldByName('ENTRADA2').AsDateTime) = '00:00', '', FormatDateTime('HH:MM', Jornada.FieldByName('ENTRADA2').AsDateTime));    &#13;&#10;    Memo36.Text := iif(FormatDateTime('HH:MM', Jornada.FieldByName('SAIDA2').AsDateTime) = '00:00', '', FormatDateTime('HH:MM', Jornada.FieldByName('SAIDA2').AsDateTime));&#13;&#10;  //  Memo42.Text := iif(FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger))) = '00:00', '',&#13;&#10;   //                    FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger))));&#13;&#10;   &#13;&#10;                 &#13;&#10;    ACodDia := DayOfWeek(&#60;Jornada.&#34;DIA_REFERENCIA&#34;&#62;);&#13;&#10;    case ACodDia of&#13;&#10;      1 : begin           &#13;&#10;            MemCodigoHorario.Text := 'Domingo';&#13;&#10;            if (Jornada.FieldByName('FALTA_JUSTIFICADA').AsString &#60;&#62; 'S') and (Jornada.FieldByName('OBSERVACAO').AsString &#60;&#62; 'FERIADO') then&#13;&#10;            begin                        &#13;&#10;              if Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger &#62;= MinutosDomingo then&#13;&#10;              begin&#13;&#10;                QntDivergenteExtra := QntDivergenteExtra + 1;                                                                       &#13;&#10;                Minutos := Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger - MinutosDomingo;&#13;&#10;                if Minutos &#62; 10 then                                                &#13;&#10;                  MinutosExtras := MinutosExtras + Minutos&#13;&#10;                else&#13;&#10;                  MinutosTotaisExtras := MinutosTotaisExtras + Minutos;                                                     &#13;&#10;                Memo42.Text := FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Minutos)));&#13;&#10;                Memo42.Font.Color := clWindowText;&#13;&#10;                if Minutos &#62; 10 then ComFundo else SemFundo;                                                                                                                                                               &#13;&#10;              end else&#13;&#10;              begin&#13;&#10;                QntDivergenteFaltantes := QntDivergenteFaltantes + 1;                                               &#13;&#10;                Minutos := MinutosDomingo - Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger;&#13;&#10;                if Minutos &#62; 10 then                                                &#13;&#10;                  MinutosFaltantes := MinutosFaltantes + Minutos&#13;&#10;                else&#13;&#10;                  MinutosTotaisFaltantes := MinutosTotaisFaltantes + Minutos;                                    &#13;&#10;                Memo42.Text := FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Minutos)));&#13;&#10;                Memo42.Font.Color := clRed;&#13;&#10;                if Minutos &#62; 10 then ComFundoFaltantes else SemFundo;                                  &#13;&#10;              end;&#13;&#10;              if Jornada.FieldByName('FALTA_JUSTIFICADA').AsString = 'N' then&#13;&#10;                MemListaHorarios.Text := Jornada.FieldByName('OBSERVACAO').AsString;                  &#13;&#10;            end else&#13;&#10;            begin                        &#13;&#10;              Memo42.Text := '';&#13;&#10;              MemListaHorarios.Text := Jornada.FieldByName('OBSERVACAO').AsString;                  &#13;&#10;            end;                                        &#13;&#10;          end;                                                                                     &#13;&#10;      2 : begin                                                                    &#13;&#10;            MemCodigoHorario.Text := 'Segunda-Feira';&#13;&#10;            if Jornada.FieldByName('OBSERVACAO').AsString = 'FERIADO' then              &#13;&#10;              MemListaHorarios.Text := 'FERIADO';                &#13;&#10;            if Jornada.FieldByName('FALTA_JUSTIFICADA').AsString &#60;&#62; 'S' then&#13;&#10;            begin                &#13;&#10;              if Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger &#62;= MinutosSegunda then&#13;&#10;              begin&#13;&#10;                QntDivergenteExtra := QntDivergenteExtra + 1;                                               &#13;&#10;                Minutos := Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger - MinutosSegunda;&#13;&#10;                if Minutos &#62; 10 then                                                &#13;&#10;                  MinutosExtras := MinutosExtras + Minutos&#13;&#10;                else&#13;&#10;                  MinutosTotaisExtras := MinutosTotaisExtras + Minutos;  &#13;&#10;                Memo42.Text := FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Minutos)));&#13;&#10;                Memo42.Font.Color := clWindowText;&#13;&#10;                if Minutos &#62; 10 then ComFundo else SemFundo;                                  &#13;&#10;              end else&#13;&#10;              begin&#13;&#10;                QntDivergenteFaltantes := QntDivergenteFaltantes + 1;                                               &#13;&#10;                Minutos := MinutosSegunda - Jornada.FieldByName('MINUTOS_TRABALHADOS').AsInteger;&#13;&#10;                if Minutos &#62; 10 then                                                &#13;&#10;                  MinutosFaltantes := MinutosFaltantes + Minutos&#13;&#10;                else&#13;&#10;                  MinutosTotaisFaltantes := MinutosTotaisFaltantes + Minutos;  &#13;&#10;                Memo42.Text := FormatDateTime('HH:MM', StrToDateTime(MinutosToHoras(Minutos)));&#13;&#10;                Memo42.Font.Color := clRed;&#13;&#10;                if Minutos &#62; 10 then ComFundoFaltantes else SemFundo;                                  &#13;&#10;              end;&#13;&#10;              if Jornada.FieldByName('FALTA_JUSTIFICADA').AsString = 'N' then&#13;&#10;                MemListaHorarios.Text := Jornada.FieldByName('OBSERVACAO').AsString;                  &#13;&#10;            end else&#13;&#10;            begin                        &#13;&#10;              Memo42.Text := '';&#13;&#10;              MemListaHorarios.Text := Jornada.FieldByName('OBSERVACAO').AsString;                  &#13;&#10;            end