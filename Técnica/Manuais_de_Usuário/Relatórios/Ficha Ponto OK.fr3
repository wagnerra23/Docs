    <TfrxIBXQuery Name="Horarios" UserName="Horarios" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select *&#13;&#10;from (select FH.CODFUNCIONARIO, '1 - Segunda-Feira' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_SEGUNDA_INICIAL as Inicio1, FH.MAT_SEGUNDA_FINAL as Final1,&#13;&#10;             FH.VES_SEGUNDA_INICIAL as Inicio2, FH.VES_SEGUNDA_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '2 - Terça-Feira' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_TERCA_INICIAL as Inicio1, FH.MAT_TERCA_FINAL as Final1, FH.VES_TERCA_INICIAL as Inicio2,&#13;&#10;             FH.VES_TERCA_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '3 - Quarta-Feira' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_QUARTA_INICIAL as Inicio1, FH.MAT_QUARTA_FINAL as Final1, FH.VES_QUARTA_INICIAL as Inicio2,&#13;&#10;             FH.VES_QUARTA_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '4 - Quinta-Feira' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_QUINTA_INICIAL as Inicio1, FH.MAT_QUINTA_FINAL as Final1, FH.VES_QUINTA_INICIAL as Inicio2,&#13;&#10;             FH.VES_QUINTA_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '5 - Sexta-Feira' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_SEXTA_INICIAL as Inicio1, FH.MAT_SEXTA_FINAL as Final1, FH.VES_SEXTA_INICIAL as Inicio2,&#13;&#10;             FH.VES_SEXTA_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '6 - Sábado' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_SABADO_INICIAL as Inicio1, FH.MAT_SABADO_FINAL as Final1, FH.VES_SABADO_INICIAL as Inicio2,&#13;&#10;             FH.VES_SABADO_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH&#13;&#10;      union&#13;&#10;      select FH.CODFUNCIONARIO, '7 - Domingo' as CODIGO_HORARIO,&#13;&#10;             FH.MAT_DOMINGO_INICIAL as Inicio1, FH.MAT_DOMINGO_FINAL as Final1,&#13;&#10;             FH.VES_DOMINGO_INICIAL as Inicio2, FH.VES_DOMINGO_FINAL as Final2&#13;&#10;      from FUNCIONARIO_HORARIO FH)             " PropData="05704C65667403F4000470546F700388000A506172616D65746572730100"/>
    <TfrxIBXQuery Name="Lista_horarios" UserName="Lista_horarios" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select FA.CODPESSOA_FUNCIONARIO,&#13;&#10;       list(lpad(extract(hour from FA.DATA_HORARIO_MARCACAO), 2, '0') || ':' || lpad(extract(minute from FA.DATA_HORARIO_MARCACAO), 2, '0'), ', ')&#13;&#10;from FUNCIONARIO_PONTO_ARQUIVO FA&#13;&#10;    " PropData="05704C65667403F4000470546F7003BC000A506172616D65746572730100"/>
    <TfrxIBXQuery Name="Funcionario" UserName="Funcionario" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select *&#13;&#10;from PESSOAS P&#13;&#10;where P.CODIGO = :CODFUNCIONARIO    " PropData="05704C65667403F4000470546F7002540A506172616D65746572730101062B204E616D653D22434F4446554E43494F4E4152494F222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxIBXQuery Name="Jornada" UserName="Jornada" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select extract(day from FP.DIA_REFERENCIA) as DIA, FP.DIA_REFERENCIA, FP.ENTRADA1, FP.SAIDA1,&#13;&#10;       FP.ENTRADA2, FP.SAIDA2, coalesce(datediff(minute from FP.ENTRADA1 to FP.SAIDA1), 0) +        &#13;&#10;                             coalesce(datediff(minute from FP.ENTRADA2 to FP.SAIDA2), 0) as MINUTOS_TRABALHADOS       &#13;&#10;from FUNCIONARIO_PONTO FP    " PropData="05704C65667403F4000470546F7003F4000A506172616D65746572730100"/>
    <TfrxIBXQuery Name="Horarios_Alterados" UserName="Horarios_Alterados" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select 'I' as OCORRENCIA, fa.*&#13;&#10;from FUNCIONARIO_PONTO_ARQUIVO FA&#13;&#10;where FA.DATA_HORARIO_MARCACAO between :DTINICIO and :DTFIM&#13;&#10;      and FA.CODPESSOA_FUNCIONARIO = :CODFUNCIONARIO&#13;&#10;      and fa.tipo_registro = 3&#13;&#10;      and extract(day from FA.DATA_HORARIO_MARCACAO) = :dia&#13;&#10;      and fa.ocorrencia in ('D', 'I')          " PropData=""/>
